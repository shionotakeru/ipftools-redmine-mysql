            <xml-property name="queryText"><![CDATA[SELECT
	CHILD.TICKET_ID AS TICKET_ID
	,CHILD.PERIOD_ZONE AS PERIOD_ZONE
	,CHILD.PERIOD_ZONE_NAME AS PERIOD_ZONE_NAME
	,CHILD.SEVERITY_ID AS SEVERITY_ID
	,CHILD.SEVERITY_NAME AS SEVERITY_NAME
	,CHILD.UNSOLVED_BUG_COUNT AS UNSOLVED_BUG_COUNT
FROM
(
	SELECT
		MART.TICKET_ID AS TICKET_ID
		,MART.PERIOD_ZONE AS PERIOD_ZONE
	FROM
		R_S11_MART MART
	WHERE
		MART.TICKET_ID	= ?
		AND MART.PROJECT_ID		=	?
	GROUP BY
		MART.TICKET_ID
		,MART.PERIOD_ZONE
	ORDER BY
		MART.PERIOD_ZONE
	OFFSET ? LIMIT ?
) PARENT
INNER JOIN
	(
		SELECT
			MART.TICKET_ID AS TICKET_ID
			,MART.PERIOD_ZONE AS PERIOD_ZONE
			,MART.PERIOD_ZONE_NAME AS PERIOD_ZONE_NAME
			,MART.SEVERITY_ID AS SEVERITY_ID
			,MART.SEVERITY_NAME AS SEVERITY_NAME
			,MART.UNSOLVED_BUG_COUNT AS UNSOLVED_BUG_COUNT
		FROM
			R_S11_MART MART
		WHERE
			MART.TICKET_ID	= ?
			AND MART.PROJECT_ID		=	?
		ORDER BY
			MART.PERIOD_ZONE
	) CHILD
ON
	PARENT.TICKET_ID = CHILD.TICKET_ID
	AND PARENT.PERIOD_ZONE = CHILD.PERIOD_ZONE
ORDER BY
	CHILD.TICKET_ID
	,CHILD.PERIOD_ZONE
	,CHILD.SEVERITY_ID

]]></xml-property>
