// 文字数を取得する処理（半角1文字）
function getStrLength(str){
	var len = 0;
	var tempStr = escape(str);

	for(var i = 0; i < tempStr.length; i++, len++){
		if(tempStr.charAt(i) == "%"){
			if(tempStr.charAt(++i) == "u"){
				i += 3;
				len++;
			}
			i++;
		}
	}
	return len;
}]]></method>
