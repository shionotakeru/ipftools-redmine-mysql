            <xml-property name="queryText"><![CDATA[SELECT
	MART.TICKET_ID AS TICKET_ID
	,MART.PROJECT_ID AS PROJECT_ID
	,MART.SUBJECT AS SUBJECT
	,MART.THEDATE AS THEDATE
	,SUM(MART.BUG_COUNT) AS BUG_COUNT 
	,SUM(MART.UNSOLVED_BUG_COUNT) AS  UNSOLVED_BUG_COUNT
	,SUM(MART.PLANNED_BUG_COUNT) AS PLANNED_BUG_COUNT
	,MART.PARENT_ID AS PARENT_ID
	,MART.TERMINAL_FLG AS TERMINAL_FLG
FROM
	R_S08_MART MART
WHERE
	MART.PROJECT_ID		=	?
	AND	MART.TICKET_ID	=	?
	AND	MART.THEDATE	BETWEEN ? AND ?
	AND	MART.PARENT_ID	=	?
GROUP BY
	MART.TICKET_ID
	,MART.PROJECT_ID
	,MART.SUBJECT
	,MART.THEDATE
	,MART.PARENT_ID
	,MART.TERMINAL_FLG
ORDER BY
	MART.THEDATE
OFFSET ? LIMIT ?]]></xml-property>
