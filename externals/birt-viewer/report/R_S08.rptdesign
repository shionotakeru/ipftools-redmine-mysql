            <xml-property name="queryText"><![CDATA[SELECT
	CHILD.TICKET_ID AS TICKET_ID
	,CHILD.PROJECT_ID AS PROJECT_ID
	,CHILD.SUBJECT AS SUBJECT
	,CHILD.THEDATE AS THEDATE
	,CHILD.BUG_COUNT AS BUG_COUNT 
	,CHILD.UNSOLVED_BUG_COUNT AS  UNSOLVED_BUG_COUNT
	,CHILD.SEVERITY_ID AS SEVERITY_ID
	,CHILD.SEVERITY_NAME AS SEVERITY_NAME
	,CHILD.PARENT_ID AS PARENT_ID
	,CHILD.TERMINAL_FLG AS TERMINAL_FLG
 FROM
(
	SELECT
		MART1.TICKET_ID AS TICKET_ID
		,MART1.PROJECT_ID AS PROJECT_ID
		,MART1.THEDATE AS THEDATE
	FROM
		R_S08_MART MART1
	WHERE
		MART1.PROJECT_ID	=	?
		AND	MART1.TICKET_ID	=	?
		AND	MART1.THEDATE	BETWEEN ? AND ?
		AND	MART1.PARENT_ID	=	?
	GROUP BY
		MART1.TICKET_ID
		,MART1.PROJECT_ID
		,MART1.THEDATE
	ORDER BY
		MART1.THEDATE
	OFFSET ? LIMIT ?
) PARENT
INNER JOIN
	(
		SELECT
			MART2.TICKET_ID AS TICKET_ID
			,MART2.PROJECT_ID AS PROJECT_ID
			,MART2.SUBJECT AS SUBJECT
			,MART2.THEDATE AS THEDATE
			,MART2.BUG_COUNT AS BUG_COUNT 
			,MART2.UNSOLVED_BUG_COUNT AS  UNSOLVED_BUG_COUNT
			,MART2.SEVERITY_ID AS SEVERITY_ID
			,MART2.SEVERITY_NAME AS SEVERITY_NAME
			,MART2.PARENT_ID AS PARENT_ID
			,MART2.TERMINAL_FLG AS TERMINAL_FLG
		FROM
			R_S08_MART MART2
		WHERE
			MART2.PROJECT_ID	=	?
			AND	MART2.TICKET_ID	=	?
			AND	MART2.THEDATE	BETWEEN ? AND ?
			AND	MART2.PARENT_ID	=	?
	) CHILD
ON
	PARENT.TICKET_ID = CHILD.TICKET_ID
	AND PARENT.PROJECT_ID = CHILD.PROJECT_ID
	AND PARENT.THEDATE = CHILD.THEDATE
ORDER BY CHILD.THEDATE]]></xml-property>
