            <xml-property name="queryText"><![CDATA[SELECT
	UNION_TABLE.TICKET_ID AS TICKET_ID
	,UNION_TABLE.SUBJECT AS SUBJECT
FROM

(
	SELECT
		'0' AS NO_DATA_FLG
		,MART.TICKET_ID AS TICKET_ID
		,MART.SUBJECT AS SUBJECT
	FROM
		R_S08_MART MART
	WHERE
		MART.PROJECT_ID	= ?
		AND MART.PARENT_ID	=	?
	GROUP BY 
		MART.TICKET_ID,
		MART.SUBJECT
	UNION
	SELECT
		'1' AS NO_DATA_FLG
		,0 AS TICKET_ID
		,? AS SUBJECT
) UNION_TABLE

WHERE 
CASE WHEN 
(
	SELECT
		COUNT(TABLE1.TICKET_ID) AS MART_COUNT
	FROM
		(
			SELECT
				MART.TICKET_ID AS TICKET_ID,
				MART.SUBJECT AS SUBJECT
			FROM
				R_S08_MART MART
			WHERE
				MART.PROJECT_ID	= ?
				AND MART.PARENT_ID	=	?
			GROUP BY 
				MART.TICKET_ID,
				MART.SUBJECT
		) TABLE1
) > 0 
	THEN UNION_TABLE.NO_DATA_FLG = '0'
	ELSE UNION_TABLE.NO_DATA_FLG = '1'
	END
ORDER BY
	UNION_TABLE.TICKET_ID]]></xml-property>
