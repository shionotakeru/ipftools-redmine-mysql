  <Script>/**&#xD;
 * Called before rendering the Title of an Axis.&#xD;
 * &#xD;
 * @param axis&#xD;
 *            Axis&#xD;
 * @param label&#xD;
 *            Title&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function beforeDrawAxisTitle( axis, label, icsc )&#xD;
{&#xD;
	// 軸タイトルのフォントサイズ変更&#xD;
	// フラグ取得&#xD;
	var navi_flg = icsc.getExternalContext().getScriptable().getParameterValue(&quot;NAVI_CREATE_FLAG&quot;);&#xD;
	// 縮小率取得&#xD;
	var Reduction = (icsc.getExternalContext().getScriptable().getPageVariable('Reduction') - 0);&#xD;
	// フラグがfalseになっている場合にフォントサイズを縮小する&#xD;
	if (navi_flg == false) {&#xD;
		var size = label.getCaption().getFont().getSize();&#xD;
		size = size / Reduction;&#xD;
		label.getCaption().getFont().setSize(size);&#xD;
	} &#xD;
}&#xD;
&#xD;
&#xD;
/**&#xD;
 * Called before generation of chart model to GeneratedChartState.&#xD;
 * &#xD;
 * @param chart&#xD;
 *            Chart&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function beforeGeneration( chart, icsc )&#xD;
{&#xD;
	// グラフタイトルのフォントサイズ変更&#xD;
	// フラグ取得&#xD;
	var navi_flg = icsc.getExternalContext().getScriptable().getParameterValue(&quot;NAVI_CREATE_FLAG&quot;);&#xD;
	// 縮小率取得&#xD;
	var Reduction = (icsc.getExternalContext().getScriptable().getPageVariable('Reduction') - 0);&#xD;
	// フラグがfalseになっている場合にフォントサイズを縮小する&#xD;
	if (navi_flg == false) {&#xD;
		var size = chart.getTitle().getLabel().getCaption().getFont().getSize();&#xD;
		size = size / Reduction;&#xD;
		chart.getTitle().getLabel().getCaption().getFont().setSize(size);&#xD;
	} &#xD;
&#xD;
}&#xD;
&#xD;
&#xD;
/**&#xD;
 * Called before drawing each marker.&#xD;
 * &#xD;
 * @param marker&#xD;
 *            Marker&#xD;
 * @param dph&#xD;
 *            DataPointHints&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function beforeDrawMarker( marker, dph, icsc )&#xD;
{&#xD;
	// グラフ系列マーカーのサイズ変更&#xD;
	// フラグ取得&#xD;
	var navi_flg = icsc.getExternalContext().getScriptable().getParameterValue(&quot;NAVI_CREATE_FLAG&quot;);&#xD;
	// 縮小率取得&#xD;
	var Reduction = (icsc.getExternalContext().getScriptable().getPageVariable('Reduction') - 0);&#xD;
	// フラグがfalseになっている場合にマーカーサイズを縮小する&#xD;
	if (navi_flg == false) {&#xD;
		var size = marker.getSize();&#xD;
		size = size / Reduction;&#xD;
		marker.setSize(size);&#xD;
	} &#xD;
	&#xD;
}&#xD;
&#xD;
&#xD;
/**&#xD;
 * Called before rendering Series.&#xD;
