    <method name="initialize"><![CDATA[importPackage(Packages.java.util);
importPackage(Packages.java.io);
importPackage(Packages.java.lang);

var prop = null;
var inputStream = null;
var path = reportContext.getResource("resource/R_M02.properties").getPath();
// プロパティファイル読込
try {   
    inputStream = new BufferedInputStream(new FileInputStream(path));
    prop = new Properties();
    prop.load(inputStream);
} finally {
    if (inputStream != null) {
        inputStream.close();
    }
}

//X軸ラベル設定値取得
vars["char_count"] = prop.getProperty("main.xaxis.label.char.by.line");
if(vars["char_count"] == null){
	vars["char_count"] = 10;
}]]></method>
