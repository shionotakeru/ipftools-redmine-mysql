            <xml-property name="queryText"><![CDATA[SELECT
	UNION_TABLE.PERIOD_ZONE AS PERIOD_ZONE
	,UNION_TABLE.PERIOD_ZONE_NAME AS PERIOD_ZONE_NAME
FROM

(
	SELECT
		'0' AS NO_DATA_FLG
		,MART.PERIOD_ZONE AS PERIOD_ZONE
		,MART.PERIOD_ZONE_NAME AS PERIOD_ZONE_NAME
	FROM
		R_S14_MART MART
	WHERE
		MART.PROJECT_ID = ?
	GROUP BY 
		MART.PERIOD_ZONE
		,MART.PERIOD_ZONE_NAME
	UNION
	SELECT
		'1' AS NO_DATA_FLG
		,0 AS PERIOD_ZONE
		,? AS PERIOD_ZONE_NAME
) UNION_TABLE

WHERE 
CASE WHEN 
(
	SELECT
		COUNT(TABLE1.PERIOD_ZONE) AS MART_COUNT
	FROM
		(
			SELECT
				MART.PERIOD_ZONE AS PERIOD_ZONE
				,MART.PERIOD_ZONE_NAME AS PERIOD_ZONE_NAME
			FROM
				R_S14_MART MART
			WHERE
				MART.PROJECT_ID = ?
			GROUP BY 
				MART.PERIOD_ZONE
				,MART.PERIOD_ZONE_NAME
		) TABLE1
) > 0 
	THEN UNION_TABLE.NO_DATA_FLG = '0'
	ELSE UNION_TABLE.NO_DATA_FLG = '1'
	END
ORDER BY
	UNION_TABLE.PERIOD_ZONE]]></xml-property>
