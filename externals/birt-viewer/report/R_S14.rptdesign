            <xml-property name="queryText"><![CDATA[SELECT 
	CHILD.PERIOD_ZONE AS PERIOD_ZONE
	,CHILD.PERIOD_ZONE_NAME AS PERIOD_ZONE_NAME
	,CHILD.UNSOLVED_SUBJECT_COUNT AS UNSOLVED_SUBJECT_COUNT
	,CHILD.SEVERITY_ID AS SEVERITY_ID
	,CHILD.SEVERITY_NAME AS SEVERITY_NAME
FROM
(
	SELECT
		MART.PERIOD_ZONE AS PERIOD_ZONE
	FROM
		R_S14_MART MART
	WHERE
		MART.PROJECT_ID = ?
		AND MART.PERIOD_ZONE >= ?
	GROUP BY
		MART.PERIOD_ZONE
	ORDER BY 
		MART.PERIOD_ZONE
	OFFSET ? LIMIT ?
) PARENT
INNER JOIN
	(
		SELECT
			MART.PERIOD_ZONE AS PERIOD_ZONE
			,MART.PERIOD_ZONE_NAME AS PERIOD_ZONE_NAME
			,MART.SEVERITY_ID AS SEVERITY_ID
			,MART.SEVERITY_NAME AS SEVERITY_NAME
			,SUM(MART.UNSOLVED_SUBJECT_COUNT) AS UNSOLVED_SUBJECT_COUNT
		FROM
			R_S14_MART MART
		WHERE
			MART.PROJECT_ID = ?
			AND MART.PERIOD_ZONE >= ?
		GROUP BY 
			MART.PERIOD_ZONE
			,MART.PERIOD_ZONE_NAME
			,MART.SEVERITY_ID
			,MART.SEVERITY_NAME
	) CHILD
ON
	PARENT.PERIOD_ZONE = CHILD.PERIOD_ZONE
ORDER BY
	CHILD.PERIOD_ZONE
	,CHILD.SEVERITY_ID
	]]></xml-property>
