    <method name="beforeFactory"><![CDATA[// コンテキストパスを取得しBIRT内部変数へ格納
var contextPath = reportContext.getHttpServletRequest().getContextPath();
vars["Context"] = contextPath;

var createFlag = params["NAVI_CREATE_FLAG"].value;

// 生成判定
if (createFlag) {
	
	// 処理なし
	
} else {
    // サイズ変更呼び出し
	bodySize();
}



// レポートの表示サイズを縮尺率分の１に変更
function bodySize() {
	importPackage(Packages.java.util);
	importPackage(Packages.java.lang);
	// 縮尺率の取得
	var Reduction = vars["Reduction"] - 0;

	for (idx = 0; idx < 1000; idx++) {
	    // チャートサイズを変更
	    var item = this.getReportElementByID(Integer.parseInt(idx + ""));
		if (item != null) {
			var b = this.getReportElementByID(Integer.parseInt(idx + "")).getQualifiedName();
			if (b == null) {
				// width縮小処理
			    var wid = item.getWidth();
				// 取得出来ない場合は何もしない
				if (wid != null) {
					// 値と単位を分離
					var SizeN = getSizeN(wid);
					var SizeS = getSizeS(wid);
					// 値を縮小値で割ったものと単位を結合
					var strSize = (SizeN / Reduction) + SizeS
					// 縮小結果を設定
					item.setWidth(strSize);
				}
				// height縮小処理
				var hei = item.getHeight();
				// 取得出来ない場合は何もしない
				if (hei != null) {
					// 値と単位を分離
					var SizeN = getSizeN(hei);
					var SizeS = getSizeS(hei);
					// 値を縮小値で割ったものと単位を結合
					var strSize = (SizeN / Reduction) + SizeS
					// 縮小結果を設定
					item.setHeight(strSize);
				}

				if (item.getStyle != null) {
					var style = item.getStyle();
					if (style != null) {
						var fnt = style.fontSize;
					    if (fnt != null) {
							var SizeN = getSizeN(fnt);
							var SizeS = getSizeS(fnt);
							var intSize = parseInt((SizeN / Reduction) + 0.9);
//							if (SizeN > 10) {
//								var intSize = parseInt((SizeN / Reduction));
//							} else {
//								var intSize = parseInt((SizeN * 0.75));
//							}
							if (intSize == 0) {
								intSize = 1;
							}
							var strSize = intSize + SizeS
							style.fontSize = strSize;
						}
					} else {
						dummy = 0;
					}
				}
			}
			// クラス名取得
			var classname = item.getClass().getName();
			// クラスがGRIDの場合のみサイズ縮小関数を実行
			if (classname == "org.eclipse.birt.report.model.simpleapi.Grid") {
				// サイズ縮小関数実行
				GridSize(idx, Reduction);
			}
		}
	}
    masterSize();
}
// サイズ分解(値取得)
function getSizeN(strSize) {

	var res = "";
	var chr = "";
	// 文字列の先頭から数字及び"."を抽出
	for (idxN = 0; idxN < strSize.length(); idxN++) {
		chr = strSize.substring(idxN, idxN + 1);
		if (isNaN(num = parseInt(chr)) == false || chr == ".") {
			res += chr;
		}
	}
	return res;
}
// サイズ分解(単位取得)
function getSizeS(strSize) {

	var res = "";
	var chr = "";
	// 文字列の先頭から数字及び"."以外を抽出
	for (idxS = 0; idxS < strSize.length(); idxS++) {
		chr = strSize.substring(idxS, idxS + 1);
		if (isNaN(num = parseInt(chr)) == true &amp;&amp; chr != ".") {
			res += chr;
		}
	}
	return res;
}

// マスターページのレイアウト変更
function masterSize() {

	// 縮尺率の取得
	var Reduction = vars["Reduction"] - 0;

    var master = reportContext.getReportRunnable().designHandle.getDesignHandle().findMasterPage("Simple MasterPage");
    // マスターページ　レイアウト
    master.setPageType("custom");
    // レイアウトタイプ（自動）
    master.setProperty("orientation", "auto");
    // 幅
	var mp_wid = master.getProperty("width");
	if (mp_wid != null) {
		var SizeN = mp_wid.measure + 1;
		var SizeS = mp_wid.units;
//		var SizeN = getSizeN(mp_wid);
//		var SizeS = getSizeS(mp_wid);
		var strSize = (SizeN / Reduction) + SizeS
	    master.setProperty("width", strSize);
	}
	// 高さ
	var mp_hei = master.getProperty("height");
	if (mp_hei != null) {
		var SizeN = mp_hei.measure;
		var SizeS = mp_hei.units;
//		var SizeN = getSizeN(mp_hei);
//		var SizeS = getSizeS(mp_hei);
		var strSize = (SizeN / Reduction) + SizeS
	    master.setProperty("height", strSize);
	}

	// マスターページヘッダーの高さ調整
	var HeaderHeight = master.getProperty("headerHeight");
	if (HeaderHeight != null) {
		var HeaderHeightMeasure = HeaderHeight.getMeasure();
		var HeaderHeightUnits = HeaderHeight.getUnits();
		HeaderHeightMeasure = HeaderHeightMeasure / (Reduction * 2);
		var DVH = new org.eclipse.birt.report.model.api.metadata.DimensionValue(HeaderHeightMeasure, HeaderHeightUnits);
		master.setProperty("headerHeight", DVH);
	}
	// マスターページフッターの高さ調整
	var FooterHeight = master.getProperty("footerHeight");
	if (FooterHeight != null) {
		var FooterHeightMeasure = FooterHeight.getMeasure();
		var FooterHeightUnits = FooterHeight.getUnits();
		FooterHeightMeasure = FooterHeightMeasure / (Reduction * 2);
		var DVF = new org.eclipse.birt.report.model.api.metadata.DimensionValue(FooterHeightMeasure, FooterHeightUnits);
		master.setProperty("footerHeight", DVF);
	}

}
// GRID行、列縮小処理
// 引数 idx         :   Element番号
//      Reduction   :   縮小率
function GridSize(idx, Reduction) {
    // reportContextからElementを取得
	var element = reportContext.getDesignHandle().getElementByID(idx);
    // ElementからModuleを取り出し(プロパティ取得時に使用)
	var module = element.getModule();
	// Elementのスロット数を取得
	var slotcnt = element.getElement().getDefn().getSlotCount();

    // スロット数分ループ(基本的にはcolumn,rowの２つのはず)
	for (slotIdx = 0; slotIdx < slotcnt; slotIdx++) {
        // elementからSlotを取得
		var slot = element.getElement().getSlot(slotIdx);
        // Slotのメンバ数を取得
		var size = slot.getContents().size();
        // メンバ数の分だけループ(行、列の分処理を繰り返し)
		for (contIdx = 0; contIdx < size; contIdx++) {
            // Slotからメンバを１件取得
			var cont = slot.getContents().get(contIdx);
            // メンバのプロパティ(width)を取得
			var prop = cont.getProperty(module, "width");
            // 取得値がnull(メンバが行もしくは幅を設定していない)の場合は処理しない
			if (prop != null) {
                // 設定値取得
				var Measure = prop.getMeasure();
                // 単位取得
				var Units = prop.getUnits();
                // 単位が％以外の場合に処理を行う
				if (Units != "%") {
                    // 設定値再計算
					Measure = Measure / Reduction;
                    // 計算結果と単位でプロパティ用のエントリを作成
					var DV = new org.eclipse.birt.report.model.api.metadata.DimensionValue(Measure, Units);
                    // プロパティ再設定
					cont.setProperty("width", DV);
				}
			}
            // メンバのプロパティ(height)を取得
			var prop = cont.getProperty(module, "height");
            // 取得値がnull(メンバが列もしくは高さを設定していない)の場合は処理しない
			if (prop != null) {
                // 設定値取得
				var Measure = prop.getMeasure();
                // 単位取得
				var Units = prop.getUnits();
                // 単位が％以外の場合に処理を行う
				if (Units != "%") {
                    // 設定値再計算
					Measure = Measure / Reduction;
                    // 計算結果と単位でプロパティ用のエントリを作成
					var DV = new org.eclipse.birt.report.model.api.metadata.DimensionValue(Measure, Units);
                    // プロパティ再設定
					cont.setProperty("height", DV);
				}
			}
		}
	}
}
]]></method>
