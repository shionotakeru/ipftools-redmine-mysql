            <xml-property name="queryText"><![CDATA[SELECT
	PARENT.TICKET_ID AS TICKET_ID
	,PARENT.SUBJECT AS SUBJECT
	,PARENT.SORT_KEY AS SORT_KEY
	,CHILD.SEVERITY_ID AS SEVERITY_ID
	,CHILD.SEVERITY_NAME AS SEVERITY_NAME
	,CHILD.UNSOLVED_SUBJECT_COUNT AS UNSOLVED_SUBJECT_COUNT
	,PARENT.SUM_UNSOLVED_SUBJECT_COUNT AS SUM_UNSOLVED_SUBJECT_COUNT
FROM
(
	SELECT
		ROW_NUMBER() OVER (ORDER BY SUM(MART.UNSOLVED_SUBJECT_COUNT) DESC, MART.TICKET_ID) AS SORT_KEY
		,MART.TICKET_ID AS TICKET_ID
		,MART.WBS_NO || '@' || MART.SUBJECT AS SUBJECT
		,SUM(MART.UNSOLVED_SUBJECT_COUNT) AS SUM_UNSOLVED_SUBJECT_COUNT
	FROM
		R_S14_MART MART
	WHERE
		MART.PROJECT_ID = ?
		AND MART.PERIOD_ZONE = ?
	GROUP BY
		MART.TICKET_ID
		,MART.WBS_NO || '@' || MART.SUBJECT
	OFFSET ? LIMIT ?
) PARENT
INNER JOIN
	(
		SELECT 
			MART.TICKET_ID AS TICKET_ID
			,MART.SEVERITY_ID AS SEVERITY_ID
			,MART.SEVERITY_NAME AS SEVERITY_NAME
			,MART.UNSOLVED_SUBJECT_COUNT AS UNSOLVED_SUBJECT_COUNT
		FROM
			R_S14_MART MART
		WHERE
			MART.PROJECT_ID = ?
			AND MART.PERIOD_ZONE = ?
	) CHILD
ON
	PARENT.TICKET_ID = CHILD.TICKET_ID

]]></xml-property>
