  <Script>&#xD;
/**&#xD;
 * Called before rendering each label on a given Axis.&#xD;
 * &#xD;
 * @param axis&#xD;
 *            Axis&#xD;
 * @param label&#xD;
 *            Label&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function beforeDrawAxisLabel( axis, label, icsc )&#xD;
{&#xD;
	//X軸ラベル整形＆設定&#xD;
	setLabelFormat( axis, label, icsc );&#xD;
	&#xD;
}&#xD;
&#xD;
/**&#xD;
 * Called before generation of chart model to GeneratedChartState.&#xD;
 * &#xD;
 * @param chart&#xD;
 *            Chart&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function beforeGeneration( chart, icsc )&#xD;
{&#xD;
	// X軸ラベル領域の高さを設定&#xD;
	setLabelSpan(icsc, chart);&#xD;
&#xD;
}&#xD;
&#xD;
&#xD;

/**
 * Called before drawing the legend item.
 * 
 * @param lerh
 * 			  LegendEntryRenderingHints
 * @param bounds
 * 			  Bounds
 * @param icsc
 * 			  IChartScriptContext
 * @since Version 2.2.0
 */
var firstFlg = true;&#xD;
var firstValue = &quot;&quot;;&#xD;
var visibleFlg = false;&#xD;
function beforeDrawLegendItem( lerh, bounds, icsc )&#xD;
{&#xD;
	// 系列1の凡例を非表示にする処理&#xD;
	var labelValue = lerh.getLabel().getCaption().getValue(); &#xD;
	if (firstFlg) {&#xD;
		firstValue = labelValue;&#xD;
	} else {&#xD;
		if (firstValue.equals(labelValue)) {&#xD;
			visibleFlg = true;&#xD;
		}&#xD;
	}&#xD;
	&#xD;
	if (!visibleFlg) {&#xD;
		lerh.getLabel().setVisible(false);&#xD;
	} else {&#xD;
		lerh.getLabel().setVisible(true);&#xD;
	}&#xD;
	&#xD;
	firstFlg = false;&#xD;
			&#xD;
}&#xD;
&#xD;
&#xD;
/**&#xD;
 * original function : adjustment Xaxis label span.&#xD;
 * &#xD;
 * @param chart&#xD;
 *            Chart&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function setLabelSpan(icsc, chart){&#xD;
	importPackage( Packages.org.eclipse.birt.chart.model.component.impl );&#xD;
	importPackage( Packages.org.eclipse.birt.chart.model.data.impl );&#xD;
	importPackage( Packages.org.eclipse.birt.chart.util );&#xD;
	    &#xD;
    // 最大の名称文字数&#xD;
    var max = Number(icsc.getExternalContext().getScriptable().getPageVariable('char_count'));&#xD;
	&#xD;
	// 設定するlabel span値&#xD;
	var span = 11 * max; &#xD;
&#xD;
	// x軸のlabel spanを設定&#xD;
	if (max > 0) {&#xD;
		&#xD;
		// x軸取得&#xD;
		var xAxis = chart.getPrimaryBaseAxes()[0];&#xD;
		// label span設定&#xD;
		xAxis.setLabelSpan(span);&#xD;
&#xD;
	}&#xD;
}&#xD;
&#xD;
&#xD;
/**&#xD;
 * original function : adjustment Xaxis label format.&#xD;
 * &#xD;
 * @param axis&#xD;
 *            Axis&#xD;
 * @param label&#xD;
 *            Label&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function setLabelFormat( axis, label, icsc ){&#xD;
	importPackage( Packages.java.lang );&#xD;
	&#xD;
	/* x軸のラベル改行処理 */&#xD;
	&#xD;
	// x軸名称HashMap&#xD;
	var xaxisMap = (icsc.getExternalContext().getScriptable().getPageVariable('Xaxis_Label'));&#xD;
	// 一行の文字数&#xD;
	var charCount = (icsc.getExternalContext().getScriptable().getPageVariable('char_count'));&#xD;
&#xD;
	// 改行する文字数&#xD;
	var limit = charCount;&#xD;
&#xD;
	// Mapから取得した名称に置き換える処理&#xD;
	importPackage(Packages.org.eclipse.birt.chart.model.attribute);&#xD;
	if (axis.getOrientation() == Orientation.HORIZONTAL_LITERAL){&#xD;
		var labelValue = Number(label.getCaption().getValue());&#xD;
		var subject = xaxisMap.get(labelValue);&#xD;
		var sb = &quot;&quot;;&#xD;
		if(subject != null){&#xD;
&#xD;
			var subjectAray = subject.split(&quot;@&quot;);&#xD;
			&#xD;
			// &lt;WBS番号>最大値を超えた場合は省略&#xD;
			subjectAray[0] = getDisplayStr(subjectAray[0],charCount);&#xD;
			&#xD;
			// &lt;名称>最大値を超えた場合は省略&#xD;
			subjectAray[1] = getDisplayStr(subjectAray[1],charCount);&#xD;
		&#xD;
			// &lt;名称>\n&lt;WBS番号>に整形&#xD;
			sb = subjectAray[1]+&quot;\n&quot;+subjectAray[0]&#xD;
		}&#xD;
		label.getCaption().setValue(sb);&#xD;
	}&#xD;
}&#xD;
&#xD;
&#xD;
/**&#xD;
 * original function : get string that does the new line processing. &#xD;
 * &#xD;
 * @param str&#xD;
 *            base string&#xD;
 * @param limit&#xD;
 *            Length limit&#xD;
 *&#xD;
 */&#xD;
function getDisplayStr(str, limit){&#xD;
	&#xD;
	importPackage( Packages.java.lang );&#xD;
	var sb = new StringBuilder(str);&#xD;
	var escapeStr = escape(str);&#xD;
	var normalLen = 0;&#xD;
	var charcLen = 0;&#xD;
	var breakLength = 0;&#xD;
	var omitLength = 0;&#xD;
	&#xD;
	for(var cnt = 0; cnt &lt; escapeStr.length; cnt++){&#xD;
		if(escapeStr.charAt(cnt) == &quot;%&quot;){&#xD;
			if(escapeStr.charAt(++cnt) == &quot;u&quot;){&#xD;
				cnt += 3;&#xD;
				normalLen++;&#xD;
			}&#xD;
			cnt++;&#xD;
		}&#xD;
		normalLen++;&#xD;
&#xD;
		// 改行位置判定&#xD;
		if (breakLength == 0) {&#xD;
			if (normalLen > limit * 2) {&#xD;
				breakLength = charcLen;&#xD;
			}&#xD;
		}&#xD;
		// 最大文字数判定&#xD;
		if (normalLen > limit * 2) {&#xD;
			omitLength = charcLen;&#xD;
			break;&#xD;
		}&#xD;
		charcLen++;&#xD;
	}&#xD;
&#xD;
	// 最大文字数を超える分は省略&#xD;
	if (omitLength != 0) {&#xD;
		sb = new StringBuilder(str.substring(0,omitLength) + &quot;..&quot;);&#xD;
	}&#xD;
&#xD;
	return sb.toString();&#xD;
}
</Script>
