    <method name="initialize"><![CDATA[importPackage(Packages.java.util);
importPackage(Packages.java.io);

var prop = null;
var inputStream = null;
var path = reportContext.getResource("resource/R_S14.properties").getPath();
// プロパティファイル読込
try {
    inputStream = new BufferedInputStream(new FileInputStream(path));
    prop = new Properties();
    prop.load(inputStream);
} finally {
    if (inputStream != null) {
        inputStream.close();
    }
}

//X軸ラベル設定値取得
vars["char_count"] = prop.getProperty("main.detail.xaxis.label.char.by.line");
if (vars["char_count"] == null || vars["char_count"] == "") {
	vars["char_count"] = 10;
}]]></method>
