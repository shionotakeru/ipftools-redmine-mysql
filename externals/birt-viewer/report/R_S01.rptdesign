/**
 * Called before drawing each marker line in an Axis.
 * 
 * @param axis
 *            Axis
 * @param markerLine
 *            MarkerLine
 * @param icsc
 *            IChartScriptContext
 */
var cnt = 0;
function beforeDrawMarkerLine( axis, markerLine, icsc )
{
	//マーカー情報取得
	var min = (icsc.getExternalContext().getScriptable().getPageVariable('lcl'));
	var max = (icsc.getExternalContext().getScriptable().getPageVariable('ucl'));
	
	var minLabel = (icsc.getExternalContext().getScriptable().getPageVariable('lcl_label'));
	var maxLabel = (icsc.getExternalContext().getScriptable().getPageVariable('ucl_label'));

	//マーカーを特定するためのラベル取得
	var label = markerLine.getLabel().getCaption().getValue();

	//Y軸最大値取得
	var maxScale = (icsc.getExternalContext().getScriptable().getPageVariable('max'));
	
	importPackage(Packages.org.eclipse.birt.chart.model.data.impl);
	var visibleFlg = false;
	if(label == &quot;upper&quot;){	
		if(max != null){
			if(max &lt;= maxScale){
				//上限値マーカー描画
				markerLine.setValue(NumberDataElementImpl.create(max));
				markerLine.getLabel().getCaption().setValue(maxLabel);
				visibleFlg = true;
			}
		}
	}
	if(label == &quot;lower&quot;){
		if(min != null){
			//下限値マーカー描画
			markerLine.setValue(NumberDataElementImpl.create(min))
			markerLine.getLabel().getCaption().setValue(minLabel);
			visibleFlg = true;
		}
	}

	if(visibleFlg){
		markerLine.getLineAttributes().setVisible(visibleFlg);
	}else{
		markerLine.getLineAttributes().setVisible(visibleFlg);
		markerLine.getLabel().setVisible(false);
	}
}

/**
 * Called before generation of chart model to GeneratedChartState.
 * 
 * @param chart
 *            Chart
 * @param icsc
 *            IChartScriptContext
 */

function beforeGeneration( chart, icsc )
{
	importPackage(Packages.org.eclipse.birt.chart.model.data.impl);

	//Y軸最大値取得
	var maxScale = (icsc.getExternalContext().getScriptable().getPageVariable('max'));
	var min = (icsc.getExternalContext().getScriptable().getPageVariable('lcl'));
	var max = (icsc.getExternalContext().getScriptable().getPageVariable('ucl'));
	
	if(min > maxScale) {// 下限値がY軸最大値を超える場合はy軸可変
		var xAxis = chart.getPrimaryBaseAxes()[0];
		var yAxis = xAxis.getAssociatedAxes().get(0);
		// スケールの最大値を設定
		yAxis.getScale().setMax(NumberDataElementImpl.create(min));
	}
	
	// X軸ラベル領域の高さを設定
	setLabelSpan(icsc, chart);
}


/**
 * Called before rendering each label on a given Axis.
 * 
 * @param axis
 *            Axis
 * @param label
 *            Label
 * @param icsc
 *            IChartScriptContext
 */

function beforeDrawAxisLabel( axis, label, icsc )
{
	//X軸ラベル整形＆設定
	setLabelFormat( axis, label, icsc );
}

/**
 * Called before rendering the Title of an Axis.
 * 
 * @param axis
 *            Axis
 * @param label
 *            Title
 * @param icsc
 *            IChartScriptContext
 */

function beforeDrawAxisTitle( axis, label, icsc )
{
	// フラグ取得
	var navi_flg = icsc.getExternalContext().getScriptable().getParameterValue(&quot;NAVI_CREATE_FLAG&quot;);
	// 縮小率取得
	var Reduction = (icsc.getExternalContext().getScriptable().getPageVariable('Reduction') - 0);
	// フラグがfalseになっている場合にフォントサイズを縮小する
	if (navi_flg == false) {
		var size = label.getCaption().getFont().getSize();
		size = size / Reduction;
		label.getCaption().getFont().setSize(size);
	} 
}

/**
 * original function : adjustment Xaxis label span.
 * 
 * @param chart
 *            Chart
 * @param icsc
 *            IChartScriptContext
 */

function setLabelSpan(icsc, chart){
	importPackage( Packages.org.eclipse.birt.chart.model.component.impl );
	importPackage( Packages.org.eclipse.birt.chart.model.data.impl );
	importPackage( Packages.org.eclipse.birt.chart.util );
	    
    // 最大名称文字数
    var max = Number(icsc.getExternalContext().getScriptable().getPageVariable('char_count'));
	// フラグ取得
	var navi_flg = icsc.getExternalContext().getScriptable().getParameterValue(&quot;NAVI_CREATE_FLAG&quot;);
	
	var areaRange = 0;
	
	if(navi_flg == true){ 
		// 非縮小時
		areaRange = 11;
	} else {
		// 縮小時
		areaRange = 5;
	}
	
	// 設定するlabel span値
	var span = areaRange * max; 

	// x軸のlabel spanを設定
	if (max > 0) {
		
		// x軸取得
		var xAxis = chart.getPrimaryBaseAxes()[0];
		// label span設定
		xAxis.setLabelSpan(span);

	}
}


/**
 * original function : adjustment Xaxis label format.
 * 
 * @param axis
 *            Axis
 * @param label
 *            Label
 * @param icsc
 *            IChartScriptContext
 */
function setLabelFormat( axis, label, icsc ){
	
	/* x軸のラベル改行処理 */
	
	// x軸名称HashMap
	var xaxisMap = (icsc.getExternalContext().getScriptable().getPageVariable('Xaxis_Label'));
	// 一行の文字数
	var charCount = Number(icsc.getExternalContext().getScriptable().getPageVariable('char_count'));

	// Mapから取得した名称に置き換える処理
	importPackage(Packages.org.eclipse.birt.chart.model.attribute);
	if (axis.getOrientation() == Orientation.HORIZONTAL_LITERAL){
		var labelValue = Number(label.getCaption().getValue());
		var subject = xaxisMap.get(labelValue);
		var newStr = &quot;&quot;;
		if(subject != null){
			newStr = getDisplayStr(subject,charCount);
		}
		label.getCaption().setValue(newStr);
	}
}

/**
 * original function : get string that does the new line processing and the omission processing. 
 * 
 * @param str
 *            base string
 * @param limit
 *            Length limit
 *
 */
function getDisplayStr(str, limit){
	
	importPackage( Packages.java.lang );
	var sb = new StringBuilder(str);
	var escapeStr = escape(str);
	var normalLen = 0;
	var charcLen = 0;
	var breakLength = 0;
	var omitLength = 0;
	
	for(var cnt = 0; cnt &lt; escapeStr.length; cnt++){
		if(escapeStr.charAt(cnt) == &quot;%&quot;){
			if(escapeStr.charAt(++cnt) == &quot;u&quot;){
				cnt += 3;
				normalLen++;
			}
			cnt++;
		}
		normalLen++;

		// 改行位置判定
		if (breakLength == 0) {
			if (normalLen > limit * 2) {
				breakLength = charcLen;
			}
		}
		// 最大文字数判定
		if (normalLen > limit * 4) {
			omitLength = charcLen;
			break;
		}
		charcLen++;
	}

	// 最大文字数を超える分は省略
	if (omitLength != 0) {
		sb = new StringBuilder(str.substring(0,omitLength) + &quot;..&quot;);
	}
	// 改行を設定
	if (breakLength != 0) {
		sb = sb.insert(breakLength,&quot;\n&quot;);
	}

	return sb.toString();
}

/**
