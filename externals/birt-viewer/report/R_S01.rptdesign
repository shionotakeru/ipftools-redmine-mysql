            <xml-property name="queryText"><![CDATA[SELECT 
     MART.TICKET_ID AS TICKET_ID
    ,MART.SUBJECT AS SUBJECT
    ,CASE WHEN MART.ACTUAL_SLOC = 0
    	  THEN 0
    	  ELSE CAST(MART.TEST_ITEM_COUNT AS FLOAT) / (CAST(MART.ACTUAL_SLOC AS FLOAT)/1000)   
     END AS DENSTY
    ,MART.PARENT_ID AS PARENT_ID
    ,MART.TERMINAL_FLG AS TARMINAL_FLG
    ,MART.INDEX_UCL AS UCL
    ,MART.INDEX_LCL AS LCL
FROM 
    R_S01_MART MART
    INNER JOIN (
    			SELECT
                      MAX(THEDATE) AS THEDATE
                      ,PROJECT_ID
                      ,TICKET_ID
                  FROM
                      R_S01_MART
                  WHERE
                      PROJECT_ID = ?
                      AND THEDATE <= ?
                      AND CASE WHEN ? = 0
                      	      THEN TICKET_ID = ?
                      	      ELSE PARENT_ID = ?
                      	  END
                  GROUP BY PROJECT_ID,TICKET_ID
    			)TMP 
    ON(MART.PROJECT_ID = TMP.PROJECT_ID AND MART.TICKET_ID = TMP.TICKET_ID AND MART.THEDATE = TMP.THEDATE)
order by MART.TICKET_ID
OFFSET ? LIMIT ?
]]></xml-property>
