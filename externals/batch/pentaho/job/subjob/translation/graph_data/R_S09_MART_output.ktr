    <sql>SELECT
  t1.ticket_id as ticket_id
 ,t2.cause_id
 ,Case When t2.completed_date Is Not Null Then 1 Else 0 End As closed_bug_count
 ,Case When t2.completed_date Is Null Then 1 Else 0 End As open_bug_count
FROM 
  wbs_ticket t1
