  <step>
    <name>不足週ラベル行追加</name>
    <type>ScriptValueMod</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <compatible>N</compatible>
    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#47;&#47;Script here

var in_week_num   = str2num(week_num);
var in_week_count = str2num(week_count);

var in_week_label_date        = week_label_date;
var in_week_first_date        = week_first_date;
var in_week_last_date         = week_last_date;
var in_est_complete_date      = est_complete_date;

if (in_week_num == in_week_count) {
	&#47;&#47; 週ラベルの最終行の場合
	
	&#47;&#47; 週ラベルが予測完了日まであるか確認
	var diff_week_count = dateDiff(in_week_last_date, in_est_complete_date, &quot;w&quot;);
	if (diff_week_count &gt; 0) {
		&#47;&#47; 週ラベルが足りない場合
		
		&#47;&#47; 無限ループ防止(1年)
		var max_week_count = (365 * 1) &#47; 7;
		if (diff_week_count &gt; max_week_count) {
			diff_week_count = max_week_count;
		}
		
		for (var i = 0; i &lt; diff_week_count; i++) {
			var new_week_num = in_week_count + 1 + i;
			var new_week_label_date      = dateAdd(week_first_date, &quot;w&quot;, new_week_num);
			var new_week_label_next_date = dateAdd(week_first_date, &quot;w&quot;, new_week_num + 1);
			var new_week_label_prev_date = dateAdd(week_first_date, &quot;w&quot;, new_week_num - 1);
			var new_week_label_end_date  = dateAdd(new_week_label_next_date, &quot;d&quot;, -1);

    		var newRow = createRowCopy(getOutputRowMeta().size());
			newRow[getInputRowMeta().indexOfValue(&quot;week_num&quot;)]             = new java.lang.Long(new_week_num);
			newRow[getInputRowMeta().indexOfValue(&quot;week_label_date&quot;)]      = new_week_label_date;
			newRow[getInputRowMeta().indexOfValue(&quot;week_label_next_date&quot;)] = new_week_label_next_date;
			newRow[getInputRowMeta().indexOfValue(&quot;week_label_prev_date&quot;)] = new_week_label_prev_date;
			newRow[getInputRowMeta().indexOfValue(&quot;week_label_end_date&quot;)]  = new_week_label_end_date;

			newRow[getInputRowMeta().indexOfValue(&quot;planned_value_graph&quot;)]  = null;
			newRow[getInputRowMeta().indexOfValue(&quot;actual_cost_graph&quot;)]  = null;
			newRow[getInputRowMeta().indexOfValue(&quot;earned_value_wbs_graph&quot;)]  = null;
			newRow[getInputRowMeta().indexOfValue(&quot;earned_value_sloc_graph&quot;)]  = null;

			newRow[getInputRowMeta().indexOfValue(&quot;planned_week_value&quot;)]   = new java.lang.Double(0);
			newRow[getInputRowMeta().indexOfValue(&quot;est_complete_date&quot;)]  = in_est_complete_date;

			putRow(newRow);
		}
		
	}
}

function maxDate(date_a, date_b) {
	var max_date = null;
	if (date_a != null &amp;&amp; date_b != null) {
		&#47;&#47; 日付A、日付B の両方が存在する場合
		if (dateDiff(date_a, date_b, &quot;d&quot;) &gt;= 0) {
			&#47;&#47; 日付A &lt;= 日付B
			max_date = date_b;
		} else {
			&#47;&#47; 日付A &gt; 日付B
			max_date = date_a;
		}

	} else if (date_a != null) {
		&#47;&#47; 日付Aのみ存在する場合
		max_date = date_a;

	} else if (date_b != null) {
		&#47;&#47; 日付Bのみ存在する場合
		max_date = date_b;
	}
	return max_date;
}

</jsScript_script>
