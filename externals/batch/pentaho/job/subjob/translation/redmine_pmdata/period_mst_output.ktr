    <connection>RM_DB</connection>
    <sql>SELECT
  t2.customized_id as project_id
 ,CASE substring(t1.name FROM 1 FOR 1) 
    WHEN &apos;P&apos; THEN &apos;1&apos;
    WHEN &apos;T&apos; THEN &apos;2&apos;
  END as type
 ,t1.name
 ,t2.value
FROM
  custom_fields t1
  INNER JOIN custom_values t2
    ON t1.id = t2.custom_field_id
WHERE
  name LIKE &apos;%PERIOD%&apos; AND
  type = &apos;ProjectCustomField&apos; AND
  customized_id = ${PROJECT_ID}
ORDER BY
  1,2,3</sql>
