        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.util.regex.Matcher;
import java.util.regex.Pattern;
import org.apache.commons.codec.DecoderException;
import org.apache.commons.io.output.ByteArrayOutputStream;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {

	logBasic("processRow");

	Object[] r = getRow();
	
	if (r == null) {
		setOutputDone();
		return false;
	}

	String base64DecString = getInputRowMeta().getString(r, getInputRowMeta().indexOfValue("base64DecString"));
	String hexDecString = hexDecString(base64DecString);

  	Object[] outputRowData = RowDataUtil.resizeArray(r, data.outputRowMeta.size());
	outputRowData[data.outputRowMeta.indexOfValue("hexDecString")] = hexDecString;
  	putRow(data.outputRowMeta, outputRowData);

	return true;

}

/**
 * 16進数文字列をデコードする。
 *
 * @str 文字列
 * @return 16進数文字列をデコードした結果
 */
public String hexDecString(String str) throws KettleException {

    String decStr = null;
    try {
        String regex = "(\\\\x[0-9A-F][0-9A-F])|(.)|(\\s)";
        Pattern p = Pattern.compile(regex);
        Matcher m = p.matcher(str);
        ByteArrayOutputStream out = new ByteArrayOutputStream();
        while (m.find()){
            String s = m.group();
            if (s.startsWith("\\x")) {
                s = s.replaceFirst("\\\\x", "0x");
                out.write(Integer.decode(s));
            } else {
                out.write(s.getBytes("UTF-8"));
            }
        }
        decStr = out.toString("UTF-8");
    } catch (Exception e) {
        throw new KettleException(e);
    }
    return decStr;

}

