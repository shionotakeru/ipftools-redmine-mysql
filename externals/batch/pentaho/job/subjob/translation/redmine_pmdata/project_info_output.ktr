    <sql>SELECT
  t1.id as project_id
 ,t3.name
 ,t2.value
FROM
  projects t1
  INNER JOIN custom_values t2
    ON t1.id = t2.customized_id
  INNER JOIN custom_fields t3
    ON t2.custom_field_id = t3.id AND
       t3.name IN (&apos;開始日&apos;,&apos;終了日&apos;,&apos;開発開始日&apos;,&apos;開発終了日&apos;,&apos;障害起算日&apos;,&apos;課題起算日&apos;)
WHERE
  t2.customized_type = &apos;Project&apos; AND
  t1.id = ${PROJECT_ID}
ORDER BY
  t1.id,t3.name</sql>
