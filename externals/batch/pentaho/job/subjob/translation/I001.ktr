&#47;**
 * 環境変数 RM_PROJECT_ROOT のチェック
 *&#47;
function chk_rm_project_root(rm_project_root) {
	var err_msg = &quot;&quot;;
	if (rm_project_root == null || rm_project_root == &quot;&quot;) {
		err_msg = &quot;環境変数 &apos;rm_PROJECT_ROOT&apos; に redmine プロジェクトのルートディレクトを設定してください。&quot;;
	}
	else if (! isFolder(rm_project_root)) {
		err_msg = &quot;環境変数 &apos;RM_PROJECT_ROOT&apos; に設定されたパス(&quot; + rm_project_root + &quot;) に誤りがあります。&quot;;
	}
	return err_msg;
}

&#47;**
 * コマンド引数 projct_name のチェック
 *&#47;
function chk_project_name(project_name, rm_project_root) {
	var err_msg = &quot;&quot;;
	if (project_name == null || project_name == &quot;&quot;) {
		err_msg = &quot;コマンド引数に &apos;プロジェクト名&apos; を指定してください。&quot;;
	}
	else if (! isFolder(rm_project_root + &quot;&#47;&quot; + project_name)) {
		err_msg = &quot;コマンド引数の&apos;プロジェクト名&apos;(&quot; + project_name + &quot;) に誤りがあります。&quot;;
	}
	return err_msg;
}
