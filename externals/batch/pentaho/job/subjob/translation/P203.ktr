    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>
if (fileExists(lock_file)) {
	&#47;&#47; ファイルが存在する場合
	var now = new Date();
	var dateFormat = &quot;yyyy&#47;MM&#47;dd HH:mm:ss&quot;;
	var strLastModify = getLastModifiedTime(lock_file, dateFormat);
	var lastModify = str2date(strLastModify,dateFormat);
	var min = dateDiff(lastModify, now, &quot;mi&quot;);
	if (min &gt;= lock_time) {
		&#47;&#47; ロックファイルが無効の場合 → ロックファイル削除
		deleteFile(lock_file);
		println(&quot;ロックファイルを削除しました。(&quot; + lock_file + &quot;)&quot;);
	} else {
		println(&quot;ロックファイルは有効です。(&quot; + lock_file + &quot;)&quot;);
	}
}
</jsScript_script>
