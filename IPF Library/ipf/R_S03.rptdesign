            <xml-property name="queryText"><![CDATA[SELECT  ticket_id,
        subject
    FROM
        (
            SELECT '0' AS NO_DATA_FLG,
                	ticket_id,
            		subject
            	FROM	r_s02_w_mart
            	WHERE	project_id	=	?
            		AND	parent_id	=	?
            	GROUP BY	ticket_id,
            				subject
            UNION
            SELECT  '1' AS NO_DATA_FLG,
                    0 AS ticket_id,
                    ? AS subject
        )   UNION_TABLE
    WHERE 
         CASE WHEN 
                (
                    SELECT   COUNT(*)
                        FROM
                            (
                                SELECT	ticket_id,
                                		subject
                                	FROM	r_s02_w_mart
                                	WHERE	project_id	=	?
                                		AND	parent_id	=	?
                                	GROUP BY	ticket_id,
                                				subject
                            )   R_S_COUNT
                ) > 0 
            THEN   NO_DATA_FLG = '0'
            ELSE   NO_DATA_FLG = '1'
         END
	ORDER BY	ticket_id
]]></xml-property>
