  <Script>&#xD;
//SimpleDateFormat&#xD;
importPackage(Packages.java.text);&#xD;
var sdf = new SimpleDateFormat(&quot;yyyy/MM/dd&quot;);&#xD;
var sdf_w = new SimpleDateFormat(&quot;MM/dd&quot;);&#xD;
var sdf_m = new SimpleDateFormat(&quot;yyyy/MM&quot;);&#xD;
&#xD;
&#xD;
/**&#xD;
 * Called before rendering each label on a given Axis.&#xD;
 * &#xD;
 * @param axis&#xD;
 *            Axis&#xD;
 * @param label&#xD;
 *            Label&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function beforeDrawAxisLabel( axis, label, icsc )&#xD;
{&#xD;
	importPackage(Packages.org.eclipse.birt.chart.model.attribute);&#xD;
	if (axis.getOrientation() == Orientation.HORIZONTAL_LITERAL){&#xD;
	&#xD;
		var scale = (icsc.getExternalContext().getScriptable().getPageVariable('scale'));&#xD;
		var today = &quot;&quot;;&#xD;
		var value = &quot;&quot;;&#xD;
	&#xD;
		//日付ラベル整形&#xD;
		if(scale == 0){&#xD;
			//today = sdf_w.format(BirtDateTime.today())&#xD;
			value = sdf_w.format(new Date(label.getCaption().getValue().replaceAll('-', '/')));&#xD;
			label.getCaption().setValue(value);&#xD;
		}else{&#xD;
			//today = sdf_m.format(BirtDateTime.today())&#xD;
			value = sdf_m.format(new Date(label.getCaption().getValue().replaceAll('-', '/')));&#xD;
			label.getCaption().setValue(value);&#xD;
		}&#xD;
		&#xD;
	}&#xD;
&#xD;
}&#xD;
&#xD;
&#xD;
function beforeDrawSeries( series, isr, icsc )&#xD;
{&#xD;
	// フラグ取得&#xD;
	var navi_flg = icsc.getExternalContext().getScriptable().getParameterValue(&quot;NAVI_CREATE_FLAG&quot;);&#xD;
	&#xD;
	if(navi_flg == false){&#xD;
		// 縮小時はドリル不可&#xD;
		icsc.getChartInstance().getInteractivity().setEnable(false);&#xD;
	}&#xD;
}&#xD;
&#xD;
/**&#xD;
 * Called before rendering the Title of an Axis.&#xD;
 * &#xD;
 * @param axis&#xD;
 *            Axis&#xD;
 * @param label&#xD;
 *            Title&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function beforeDrawAxisTitle( axis, label, icsc )&#xD;
{&#xD;
	// フラグ取得&#xD;
	var navi_flg = icsc.getExternalContext().getScriptable().getParameterValue(&quot;NAVI_CREATE_FLAG&quot;);&#xD;
	// 縮小率取得&#xD;
	var Reduction = (icsc.getExternalContext().getScriptable().getPageVariable('Reduction') - 0);&#xD;
	// フラグがfalseになっている場合にフォントサイズを縮小する&#xD;
	if (navi_flg == false) {&#xD;
		var size = label.getCaption().getFont().getSize();&#xD;
		size = size / Reduction;&#xD;
		label.getCaption().getFont().setSize(size);&#xD;
	} &#xD;
}&#xD;
&#xD;
&#xD;
/**&#xD;
 * Called before drawing each marker.&#xD;
 * &#xD;
 * @param marker&#xD;
 *            Marker&#xD;
 * @param dph&#xD;
 *            DataPointHints&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function beforeDrawMarker( marker, dph, icsc )&#xD;
{&#xD;
	// グラフ系列マーカーのサイズ変更&#xD;
	// フラグ取得&#xD;
	var navi_flg = icsc.getExternalContext().getScriptable().getParameterValue(&quot;NAVI_CREATE_FLAG&quot;);&#xD;
	// 縮小率取得&#xD;
	var Reduction = (icsc.getExternalContext().getScriptable().getPageVariable('Reduction') - 0);&#xD;
	// フラグがfalseになっている場合にフォントサイズを縮小する&#xD;
	if (navi_flg == false) {&#xD;
		var size = marker.getSize();&#xD;
		size = size / Reduction;&#xD;
		marker.setSize(size);&#xD;
	} &#xD;
	&#xD;
}&#xD;
&#xD;
