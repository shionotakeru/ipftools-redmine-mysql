            <xml-property name="queryText"><![CDATA[select  
	  CHILD.TICKET_ID 		AS TICKET_ID
    , CHILD.SUBJECT 		AS SUBJECT
    , CHILD.PROG * 100			AS PROG
    , CHILD.PROG_DIFF * 100 		AS PROG_DIFF
    , CHILD.THEDATE			AS THEDATE
    , CHILD.PARENT_ID 		AS PARENT_ID
    , CHILD.TERMINL_FLG 	AS TERMINL_FLG
from 
(
	SELECT
		   THEDATE
	FROM
		FUNC_R_S02(?,?,'@',?,0,?) 
  	  as (
     		 TICKET_ID 			INT
    		, SUBJECT 			VARCHAR
    		, PROG 				FLOAT
    		, PROG_DIFF 		FLOAT
    		, EST_PROG 			FLOAT
    		, EST_PROG_DIFF 	FLOAT
    		, THEDATE 			DATE
    		, PARENT_ID 		INT
    		, TERMINL_FLG 		CHAR
    		, EST_FLG 			CHAR
    	 )
	GROUP BY
		THEDATE
	ORDER BY THEDATE
	OFFSET ? LIMIT ?
) PARENT
INNER JOIN
(
	SELECT
	 	  TICKET_ID
    	, SUBJECT
    	, PROG
    	, PROG_DIFF
    	, EST_PROG
    	, EST_PROG_DIFF
    	, THEDATE
    	, PARENT_ID
    	, TERMINL_FLG
    	,EST_FLG 
	FROM
		FUNC_R_S02(?,?,'@',?,0,?) 
  	  as (
     		 TICKET_ID 			INT
    		, SUBJECT 			VARCHAR
    		, PROG 				FLOAT
    		, PROG_DIFF 		FLOAT
    		, EST_PROG 			FLOAT
    		, EST_PROG_DIFF 	FLOAT
    		, THEDATE 			DATE
    		, PARENT_ID 		INT
    		, TERMINL_FLG 		CHAR
    		, EST_FLG 			CHAR
    	 )
    where EST_FLG = '0'
) CHILD
ON
	PARENT.THEDATE = CHILD.THEDATE
ORDER BY CHILD.THEDATE
]]></xml-property>
