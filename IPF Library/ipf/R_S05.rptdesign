    <method name="initialize"><![CDATA[// マーカーの上限、下限を取得
importPackage(Packages.java.util);
importPackage(Packages.java.io);

var path = reportContext.getResource("resource/R_S05.properties").getPath();
inStream = new BufferedInputStream(new FileInputStream(path));
prop = new Properties();
prop.load(inStream);

var marker_upper = prop.getProperty("main.marker.upper.value");
var marker_lower = prop.getProperty("main.marker.lower.value");
if (marker_upper != null) {
	vars["marker_upper"] = 1 + marker_upper / 100;
} else {
	vars["marker_upper"] = 1.2;
}
if (marker_lower != null) {
	vars["marker_lower"] = 1 - marker_lower / 100;
} else {
	vars["marker_lower"] = 0.8;
}]]></method>
    <method name="beforeFactory"><![CDATA[importPackage(Packages.java.util);

// コンテキストパスを取得しBIRT内部変数へ格納
var contextPath = reportContext.getHttpServletRequest().getContextPath();
vars["Context"] = contextPath;

// パンくず表示用キー
var navi = "R_S05"+ params["PROJECT_ID"].value;
//　パラメータ保持用
var breadNavi = "breadNaviList" + "R_S05"+ params["PROJECT_ID"].value;
// 必須パラメータ
var requiredParam = "&amp;__svg=false&amp;__masterpage=true";
// 空文字
var blank = "";
// 区切り文字（置換判定用）
var delimiter = ",";
// QeryString
var reportSelect = "?__report=";
// AND
var and = "&amp;";

// デザインファイル名
var reportPath = reportContext.getReportRunnable().getReportName();
reportPath = reportPath.replace("file:", "");
// リクエスト情報
var session = reportContext.getHttpServletRequest().getSession(true);
var requestURL = reportContext.getHttpServletRequest().getRequestURL();

// パンくず情報
var newWinFlag = params["NEW_WINDOW_FLAG"].value;
var createFlag = params["NAVI_CREATE_FLAG"].value;
var dispName = params["DISP_NAME"].value;
var firstFlag = params["DRILL_FIRST_FLAG"].value;
// ツリーの先頭判定
if(params["INDEX"].value == 0){
	firstFlag = true;
}

params["DRILL_FIRST_FLAG"].value = false;
var drillFlag = params["DRILL_FLAG"].value

// URL
var url = requestURL + reportSelect + reportPath;
var linkName = blank;


// 生成判定
if (createFlag) {
	//新規ウィンドウ立ち上げ判定
	if(!newWinFlag){
	    var location = url + createURLParameter();
	    if (firstFlag) {
	        removeSession(session, breadNavi);
	        // 初回処理
	        naviList = new LinkedList();
	        // リンクパラメータを生成
	        naviList.add(createAddParameter(location, params["HIDDEN_PARENT_ID"].value));
	        // 初回ドリルダウン時パラメータ設定
	        session.setAttribute(breadNavi, naviList);
	    } else if (drillFlag) {
	        // ドリルダウン処理
	        naviList = session.getAttribute(breadNavi);      
	        // 最終要素を取得
	        var tmp = naviList.peekLast();
	        var id = tmp[0];
	        var link = tmp[1];
	        var element = tmp[2];
	        var addArray = null;
	        // 階層パラメータ判定
	        if (id != params["HIDDEN_PARENT_ID"].value) { // 同階層でない場合                       
	            naviList.add(createAddParameter(location, params["HIDDEN_PARENT_ID"].value));
	            // 親名称を設定
	            tmp[1] = element.replace(delimiter, dispName);
	        }
	        linkName = createNavi(naviList);
	    } else {
	        // 戻り動作処理
	        naviList = session.getAttribute(breadNavi); 
	        naviList = removeListElement(naviList, params["INDEX"].value);
	        linkName = createNavi(naviList);
	        var lastData = naviList.pollLast();
	        var createParameData = createAddParameter(location, params["HIDDEN_PARENT_ID"].value);
	        if (lastData != createParameData) {
	        	// パラメータ再設定
	            naviList.add(createParameData);
	        } else {
	            naviList.add(lastData);
	        }
	    }
    }
} else {
	// 縮小処理
	bodySize();
}

// パンくず表示用パラメータ設定
reportContext.setPersistentGlobalVariable(navi, linkName);

// 戻り動作分要素を削除
function removeListElement(removeList, index) {
    var size = (removeList.size() - 1)  - (index);
    for (var i = 0; i < size; i++) {
        removeList.pollLast();
    }
    return removeList;
}


// パンくずリストへの追加情報
function createAddParameter(url, hierarchy) {
    var aHref = " > <a href=\"";
    var location =  aHref + url + "\">" + delimiter + "</a>"; 
    var addArray = new Array(3);
    addArray[0] = hierarchy + blank;
    addArray[1] = location;
    addArray[2] = location;
    return addArray;
}

// List<String[]> 用
function createNavi(naviList) {
    var link = blank;
    var tmp = null;
    for (var itr = naviList.iterator(); itr.hasNext();) {
        tmp = itr.next();
        // 最終要素判定
        if (!itr.hasNext()) {
            // 最終要素はパンくずリストへ追加しない
            break;
        }
        link += tmp[1];
    }
    return link;
}

// セッションの削除処理
function removeSession(sesion, removeName) {
    session.removeAttribute(removeName);
}

// 入力パラメータからURLパラメータを生成（各レポート固有パラメータ）
function createURLParameter() { 
	
    var param = requiredParam
        // ページング用パラメータ
        + and + "LIMMIT=" + params["LIMMIT"].value
        // パンくず用パラメータ
        + and + "NAVI_CREATE_FLAG=" + params["NAVI_CREATE_FLAG"].value
        + and + "DRILL_FIRST_FLAG=" + params["DRILL_FIRST_FLAG"].value
        + and + "INDEX=" + params["INDEX"].value
        // 個別パラメータ
        + and + "TICKET_ID=" + params["TICKET_ID"].value
        + and + "TARGET_DATE_FROM=" + params["TARGET_DATE_FROM"].value
        + and + "TARGET_DATE_TO=" + params["TARGET_DATE_TO"].value      
        + and + "PROJECT_ID=" + params["PROJECT_ID"].value
        + and + "USER_ID=" + params["USER_ID"].value
        + and + "HELP_FLAG=" + params["HELP_FLAG"].value
        + and + "HIDDEN_PARENT_ID=" + params["HIDDEN_PARENT_ID"].value
    ;
	 
    return param;
}

// 存在チェック
function isParamCheck(param) {
    if (param != null &amp;&amp; param &amp;&amp; param != '') {
        return true;
    }
    return false;
}
// レポートの表示サイズを縮尺率分の１に変更
function bodySize() {
	importPackage(Packages.java.util);
	importPackage(Packages.java.lang);
	// 縮尺率の取得
	var Reduction = vars["Reduction"] - 0;

	for (idx = 0; idx < 1000; idx++) {
	    // チャートサイズを変更
	    var item = this.getReportElementByID(Integer.parseInt(idx + ""));
		if (item != null) {
			var b = this.getReportElementByID(Integer.parseInt(idx + "")).getQualifiedName();
			if (b == null) {
				// width縮小処理
			    var wid = item.getWidth();
				// 取得出来ない場合は何もしない
				if (wid != null) {
					// 値と単位を分離
					var SizeN = getSizeN(wid);
					var SizeS = getSizeS(wid);
					// 値を縮小値で割ったものと単位を結合
					var strSize = (SizeN / Reduction) + SizeS
					// 縮小結果を設定
					item.setWidth(strSize);
				}
				// height縮小処理
				var hei = item.getHeight();
				// 取得出来ない場合は何もしない
				if (hei != null) {
					// 値と単位を分離
					var SizeN = getSizeN(hei);
					var SizeS = getSizeS(hei);
					// 値を縮小値で割ったものと単位を結合
					var strSize = (SizeN / Reduction) + SizeS
					// 縮小結果を設定
					item.setHeight(strSize);
				}

				if (item.getStyle != null) {
					var style = item.getStyle();
					if (style != null) {
						var fnt = style.fontSize;
					    if (fnt != null) {
							var SizeN = getSizeN(fnt);
							var SizeS = getSizeS(fnt);
							var intSize = parseInt((SizeN / Reduction) + 0.9);
//							if (SizeN > 10) {
//								var intSize = parseInt((SizeN / Reduction));
//							} else {
//								var intSize = parseInt((SizeN * 0.75));
//							}
							if (intSize == 0) {
								intSize = 1;
							}
							var strSize = intSize + SizeS
							style.fontSize = strSize;
						}
					} else {
						dummy = 0;
					}
				}
			}
		}
	}
    masterSize();
}
// サイズ分解(値取得)
function getSizeN(strSize) {

	var res = "";
	var chr = "";
	// 文字列の先頭から数字及び"."を抽出
	for (idxN = 0; idxN < strSize.length(); idxN++) {
		chr = strSize.substring(idxN, idxN + 1);
		if (isNaN(num = parseInt(chr)) == false || chr == ".") {
			res += chr;
		}
	}
	return res;
}
// サイズ分解(単位取得)
function getSizeS(strSize) {

	var res = "";
	var chr = "";
	// 文字列の先頭から数字及び"."以外を抽出
	for (idxS = 0; idxS < strSize.length(); idxS++) {
		chr = strSize.substring(idxS, idxS + 1);
		if (isNaN(num = parseInt(chr)) == true &amp;&amp; chr != ".") {
			res += chr;
		}
	}
	return res;
}

// マスターページのレイアウト変更
function masterSize() {
	// 縮尺率の取得
	var Reduction = vars["Reduction"] - 0;
    var master = reportContext.getReportRunnable().designHandle.getDesignHandle().findMasterPage("NewSimpleMasterPage");
    // マスターページ　レイアウト
    //master.setPageType("custom");
    // レイアウトタイプ（自動）
    //master.setProperty("orientation", "auto");
    // 高さ
	var mp_wid = master.getProperty("width");
	if (mp_wid != null) {
		var SizeN = getSizeN(mp_wid + "");
		var SizeS = getSizeS(mp_wid + "");
		var strSize = (SizeN / Reduction + 0.4) + SizeS
	    master.setProperty("width", strSize);
	}
	// 幅
	var mp_hei = master.getProperty("height");
	if (mp_hei != null) {
		var SizeN = getSizeN(mp_hei + "");
		var SizeS = getSizeS(mp_hei + "");
		var strSize = (SizeN / Reduction) + SizeS
	    master.setProperty("height", strSize);
	}
}
