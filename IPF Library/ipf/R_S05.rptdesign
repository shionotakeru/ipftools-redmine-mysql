/**
 * Called before drawing each marker line in an Axis.
 * 
 * @param axis
 *            Axis
 * @param markerLine
 *            MarkerLine
 * @param icsc
 *            IChartScriptContext
 */

function beforeDrawMarkerLine( axis, markerLine, icsc )
{
	// マーカー線出力位置制御

	//マーカー情報取得
	var max = icsc.getExternalContext().getScriptable().getPageVariable('marker_upper');
	var min = icsc.getExternalContext().getScriptable().getPageVariable('marker_lower');
	var rec_count = icsc.getExternalContext().getScriptable().getPageVariable('recCount') - 0;
	
	//マーカーを特定するためのラベル取得
	var label = markerLine.getLabel().getCaption().getValue();
	//Y軸最大値取得
	var maxScale = icsc.getExternalContext().getScriptable().getPageVariable('yaxis_max');
	// 最大値が2.0を下回った時は2.0に置き換え
	if (maxScale &lt; 2.0) {
		maxScale = 2.0;
	}
	var visibleFlg = false;
	if(label == &quot;upper&quot;){	
		if(max != null){
			if(max &lt;= maxScale){
				//上限値マーカー描画
				markerLine.setValue(NumberDataElementImpl.create(max));
				markerLine.getLabel().getCaption().setValue(max);
				visibleFlg = true;
			}
		}
	}
	if(label == &quot;lower&quot;){	
		if(min != null){
			//下限値マーカー描画
			markerLine.setValue(NumberDataElementImpl.create(min))
			markerLine.getLabel().getCaption().setValue(min);
			visibleFlg = true;
		}
	}
	if(label == &quot;center&quot;){	
		//中央値マーカー描画
		markerLine.setValue(NumberDataElementImpl.create('1.0'));
		markerLine.getLabel().getCaption().setValue('1.0');
		visibleFlg = true;
	}
	
	if(rec_count == 0){
		markerLine.getLineAttributes().setVisible(false);
		markerLine.getLabel().setVisible(false);
	}else{
		markerLine.getLineAttributes().setVisible(visibleFlg);
		markerLine.getLabel().setVisible(false);
	}
}

/**
 * Called before generation of chart model to GeneratedChartState.
 * 
 * @param chart
 *            Chart
 * @param icsc
 *            IChartScriptContext
 */

function beforeGeneration( chart, icsc )
{
	var val = icsc.getExternalContext().getScriptable().getPageVariable('yaxis_max');

	// Y軸２スケール制御
	// Y軸固定値2を超える進捗率がある場合はy軸可変
	if(val > 2) {
		// x軸取得
		var xAxis = chart.getPrimaryBaseAxes()[0];
		// 2軸目のy軸取得
		var yAxis = xAxis.getAssociatedAxes().get(1);
		// スケールの最大値を設定
		yAxis.getScale().setMax(NumberDataElementImpl.create(val));
	}

}

