            <xml-property name="queryText"><![CDATA[SELECT  ticket_id                   AS ticket_id,
        project_id                  AS project_id,
        subject                     AS subject,
        thedate                     AS thedate,
        actual_sloc                 AS sloc,
        productivity_diff           AS productivity_diff,
        progress                    AS progress,
        parent_id                   AS parent_id,
        terminal_flg                AS terminal_flg,
        max_progress                AS max
    FROM
        (SELECT ticket_id                   AS ticket_id,
                project_id                  AS project_id,
                subject                     AS subject,
                thedate                     AS thedate,
                actual_sloc                 AS actual_sloc,
                variation                   AS productivity_diff,
                CASE WHEN planned_sloc = 0
					THEN
						CAST(0.0 AS FLOAT)
					ELSE
                		CAST(actual_sloc AS FLOAT) /
		                CAST(planned_sloc AS FLOAT)
				END							AS progress,
                parent_id                   AS parent_id,
                terminal_flg                AS terminal_flg
            FROM    r_s05_mart
            WHERE   project_id  =   ?
                AND ticket_id   =   ?
                AND thedate BETWEEN ?   AND ?
            	AND	CASE WHEN ? = 0
            			 THEN ticket_id = ?
            			 ELSE parent_id = ?
            		END
        )   r_s05_mart_data
        ,
        (SELECT MAX(CASE WHEN planned_sloc = 0
						THEN
							CAST(0 AS FLOAT)
						ELSE
					        CAST(actual_sloc AS FLOAT) /
                    		CAST(planned_sloc AS FLOAT)
                    END)  AS max_progress
            FROM    r_s05_mart
            WHERE   project_id  =   ?
                AND ticket_id   =   ?
                AND thedate BETWEEN ?   AND ?
            	AND	CASE WHEN ? = 0
            			 THEN ticket_id = ?
            			 ELSE parent_id = ?
            		END
        )   r_s05_mart_max
    ORDER BY    ticket_id
OFFSET	?	LIMIT	?
]]></xml-property>
