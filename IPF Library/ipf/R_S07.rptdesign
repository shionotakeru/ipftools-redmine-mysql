	}&#xD;
}&#xD;

// 当日を含む週判定&#xD;
// パラメータの日付から１週間に当日を含む場合はtrue&#xD;
// それ以外はfalseを返却&#xD;
function diffDate(scale, capDate) {&#xD;
	if (scale == 0) {&#xD;
		var diff = BirtDateTime.diffDay(capDate, BirtDateTime.today())&#xD;
		if(diff >= 0 &amp;&amp; diff &lt;= 6){&#xD;
			return true;&#xD;
		} else {&#xD;
			return false;&#xD;
		}&#xD;
	} else {&#xD;
		var sdf = new SimpleDateFormat(&quot;yyyy/MM&quot;);&#xD;
		if (capDate.substring(0, 7) == sdf.format(BirtDateTime.today())) {&#xD;
			return true;&#xD;
		} else {&#xD;
			return false;&#xD;
		}&#xD;
	}&#xD;
}&#xD;
