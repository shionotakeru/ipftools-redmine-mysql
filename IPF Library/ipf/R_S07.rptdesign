&#xD;
/**&#xD;
 * Called before rendering each label on a given Axis.&#xD;
 * &#xD;
 * @param axis&#xD;
 *            Axis&#xD;
 * @param label&#xD;
 *            Label&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function beforeDrawAxisLabel( axis, label, icsc )&#xD;
{&#xD;
	importPackage(Packages.org.eclipse.birt.chart.model.component.impl);&#xD;
	importPackage(Packages.org.eclipse.birt.chart.model.data.impl);&#xD;
	importPackage(Packages.org.eclipse.birt.chart.model.attribute);&#xD;
	importPackage(Packages.org.eclipse.birt.chart.model.attribute.impl);&#xD;
&#xD;
	// タスク毎に色を交互に切り替える&#xD;
	if (axis.getOrientation() == Orientation.HORIZONTAL_LITERAL){&#xD;
		// フラグ初期化&#xD;
		var colorflg = 0;&#xD;
		// x軸色判定フラグHashMap&#xD;
		var xaxisColor = (icsc.getExternalContext().getScriptable().getPageVariable('Xaxis_Color'));&#xD;
		// 日付　+　ticket_id(色設定用)+　ticket_id(見出し設定用)の形式のラベルからticket_id(色設定用)を取得&#xD;
		// ラベル文字列取得&#xD;
		var labelValue = label.getCaption().getValue();&#xD;
		// 文字列を分解&#xD;
		var labelAry = labelValue.split('@');&#xD;
		// 分解した２つ目(ticket_id)をキーとしてmapを検索&#xD;
		var flg = xaxisColor.get(Number(labelAry[1]));&#xD;
		// 見つかった場合は取得値を設定&#xD;
		if (flg != null) {&#xD;
			colorflg = flg;&#xD;
		}&#xD;
		// フラグにより色を設定&#xD;
		if (colorflg == 0) {&#xD;
			label.getCaption().getColor().set(0,0,255);&#xD;
		} else {&#xD;
			label.getCaption().getColor().set(0,0,0);&#xD;
		}&#xD;
	}&#xD;
&#xD;
	//X軸ラベル整形＆設定&#xD;
	setLabelFormat( axis, label, icsc );&#xD;
	&#xD;
	// 当日の文字色を赤に変更&#xD;
	if (axis.getOrientation() == Orientation.HORIZONTAL_LITERAL){&#xD;
&#xD;
		importPackage(Packages.java.text);&#xD;
		var sdf = &quot;&quot;;&#xD;
		var sdf_len = 0;&#xD;
&#xD;
		// 日付部分とタスク名部分を分解する&#xD;
		var cap1 = label.getCaption().getValue().substring(0, 10);&#xD;
		var cap2 = label.getCaption().getValue().substring(10);&#xD;
&#xD;
&#xD;
		// パラメータ&quot;SCALE&quot;を取得&#xD;
		var scale = icsc.getExternalContext().getScriptable().getParameterValue(&quot;SCALE&quot;);&#xD;
		// 週の場合は&quot;MM/dd&quot;フォーマット、月の場合は&quot;YYYY/MM&quot;フォーマットに設定&#xD;
		if (scale == 0) {&#xD;
			sdf = new SimpleDateFormat(&quot;MM/dd&quot;);&#xD;
			sdf_len = 5;&#xD;
		} else {&#xD;
			sdf = new SimpleDateFormat(&quot;yyyy/MM&quot;);&#xD;
			sdf_len = 7;&#xD;
		}&#xD;
		// 編集後の日付文字列を設定&#xD;
		label.getCaption().setValue(sdf.format(new Date(cap1)) + cap2);&#xD;
&#xD;
		var labelLen = label.getCaption().getValue().length();&#xD;
		// X軸の日付が当日の場合は赤に文字色を設定&#xD;
		if(diffDate(scale, cap1)){&#xD;
			label.getCaption().getColor().set(255,0,0);&#xD;
		}&#xD;
	}&#xD;
}&#xD;
&#xD;
/**&#xD;
 * Called before generation of chart model to GeneratedChartState.&#xD;
 * &#xD;
 * @param chart&#xD;
 *            Chart&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function beforeGeneration( chart, icsc )&#xD;
{&#xD;
	// X軸ラベル領域の高さを設定&#xD;
	setLabelSpan(icsc, chart);&#xD;
}&#xD;
&#xD;
/**&#xD;
 * original function : adjustment Xaxis label span.&#xD;
 * &#xD;
 * @param chart&#xD;
 *            Chart&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function setLabelSpan(icsc, chart){&#xD;
	importPackage( Packages.org.eclipse.birt.chart.model.component.impl );&#xD;
	importPackage( Packages.org.eclipse.birt.chart.model.data.impl );&#xD;
	importPackage( Packages.org.eclipse.birt.chart.util );&#xD;
	    &#xD;
    // 最大の期間帯レベル名称文字数&#xD;
    var max = Number(icsc.getExternalContext().getScriptable().getPageVariable('char_count'));&#xD;
	// フラグ取得&#xD;
	var navi_flg = icsc.getExternalContext().getScriptable().getParameterValue(&quot;NAVI_CREATE_FLAG&quot;);&#xD;
	// 縮小率取得&#xD;
	var Reduction = (icsc.getExternalContext().getScriptable().getPageVariable('Reduction') - 0);&#xD;
	&#xD;
	// 設定するlabel span値&#xD;
	var span = 15 * max; &#xD;
	if (navi_flg == false) {&#xD;
		span = span / Reduction;&#xD;
	}&#xD;
&#xD;
	// x軸のlabel spanを設定&#xD;
	if (max > 0) {&#xD;
		// x軸取得&#xD;
		var xAxis = chart.getPrimaryBaseAxes()[0];&#xD;
		// label span設定&#xD;
		xAxis.setLabelSpan(span);&#xD;
&#xD;
	}&#xD;
}&#xD;
&#xD;
&#xD;
/**&#xD;
 * original function : adjustment Xaxis label format.&#xD;
 * &#xD;
 * @param axis&#xD;
 *            Axis&#xD;
 * @param label&#xD;
 *            Label&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function setLabelFormat( axis, label, icsc ){&#xD;
	importPackage( Packages.java.lang );&#xD;
	&#xD;
	/* x軸のラベル改行処理 */&#xD;
	&#xD;
	// x軸名称HashMap&#xD;
	var xaxisMap = (icsc.getExternalContext().getScriptable().getPageVariable('Xaxis_Label'));&#xD;
	// 一行の文字数&#xD;
	var charCount = (icsc.getExternalContext().getScriptable().getPageVariable('char_count'));&#xD;
&#xD;
	// 改行する文字数&#xD;
	var limit = charCount;&#xD;
	var key = &quot;&quot;;&#xD;
	var dateLabel = &quot;&quot;;&#xD;
	var colorkey = &quot;&quot;;&#xD;
&#xD;
	// Mapから取得した名称に置き換える処理&#xD;
	importPackage(Packages.org.eclipse.birt.chart.model.attribute);&#xD;
	if (axis.getOrientation() == Orientation.HORIZONTAL_LITERAL){&#xD;
		var labelValue = label.getCaption().getValue();&#xD;
		var sb = &quot;&quot;;&#xD;
		&#xD;
		// 日付　+　ticket_idの形式のラベルからticket_idを取得&#xD;
		var labelAry = labelValue.split('@');&#xD;
		&#xD;
		if(labelAry.length == 3){&#xD;
			dateLabel = labelAry[0];&#xD;
			key = Number(labelAry[2]);&#xD;
		} else {&#xD;
			// ticket_id が付随していない場合は日付をラベルに表示&#xD;
			sb = labelAry[0];&#xD;
		}&#xD;
		var subject = xaxisMap.get(key);&#xD;
		var newStr = labelAry[0];&#xD;
		if(subject != null){&#xD;
&#xD;
			newStr = getDisplayStr(subject&#xD;
								  , charCount&#xD;
								  , icsc.getExternalContext().getScriptable().getParameterValue(&quot;SCALE&quot;));&#xD;
			newStr = dateLabel + &quot; &quot; + newStr;&#xD;
		}&#xD;
&#xD;
		label.getCaption().setValue(newStr);&#xD;
&#xD;
	}&#xD;
}&#xD;

&#xD;
/**&#xD;
 * original function : get string that does the new line processing and the omission processing. &#xD;
 * &#xD;
 * @param str&#xD;
 *            base string&#xD;
 * @param limit&#xD;
 *            Length limit&#xD;
 *&#xD;
 */&#xD;
function getDisplayStr(str, limit, scale){&#xD;
&#xD;
	importPackage( Packages.java.lang );&#xD;
	var sb = new StringBuilder(str);&#xD;
	var escapeStr = escape(str);&#xD;
	var normalLen = 0;&#xD;
	var charcLen = 0;&#xD;
	var breakLength = 0;&#xD;
	var omitLength = 0;&#xD;
	&#xD;
	for(var cnt = 0; cnt &lt; escapeStr.length; cnt++){&#xD;
		if(escapeStr.charAt(cnt) == &quot;%&quot;){&#xD;
			if(escapeStr.charAt(++cnt) == &quot;u&quot;){&#xD;
				cnt += 3;&#xD;
				normalLen++;&#xD;
			}&#xD;
			cnt++;&#xD;
		}&#xD;
		normalLen++;&#xD;
&#xD;
		// 改行位置判定&#xD;
		if (breakLength == 0) {&#xD;
			if (normalLen > limit * 2) {&#xD;
				breakLength = charcLen;&#xD;
			}&#xD;
		}&#xD;
		// 最大文字数判定&#xD;
		if (normalLen > limit * 4) {&#xD;
			omitLength = charcLen;&#xD;
			break;&#xD;
		}&#xD;
		charcLen++;&#xD;
	}&#xD;
&#xD;
	// 最大文字数を超える分は省略&#xD;
	if (omitLength != 0) {&#xD;
		sb = new StringBuilder(str.substring(0,omitLength) + &quot;..&quot;);&#xD;
	}&#xD;
	&#xD;
	// パラメータ&quot;SCALE&quot;を取得&#xD;
	//var scale = icsc.getExternalContext().getScriptable().getParameterValue(&quot;SCALE&quot;);&#xD;
	var sp = &quot;&quot;;&#xD;
	if(scale == 0){&#xD;
		sp = &quot;　　　 &quot;;&#xD;
	} else {&#xD;
		sp = &quot;　　　     &quot;;&#xD;
	}&#xD;
	&#xD;
	// 改行を設定&#xD;
	if (breakLength != 0) {&#xD;
		sb = sb.insert(breakLength, &quot;\n&quot; + sp);&#xD;
	}&#xD;
&#xD;
	return sb.toString();&#xD;
}&#xD;


/**
 * Called before rendering the Title of an Axis.
 * 
 * @param axis
 *            Axis
 * @param label
 *            Title
 * @param icsc
 *            IChartScriptContext
 */

function beforeDrawAxisTitle( axis, label, icsc )
{
	// フラグ取得&#xD;
	var navi_flg = icsc.getExternalContext().getScriptable().getParameterValue(&quot;NAVI_CREATE_FLAG&quot;);&#xD;
	// 縮小率取得&#xD;
	var Reduction = (icsc.getExternalContext().getScriptable().getPageVariable('Reduction') - 0);&#xD;
	// フラグがfalseになっている場合にフォントサイズを縮小する&#xD;
	if (navi_flg == false) {&#xD;
		var size = label.getCaption().getFont().getSize();&#xD;
		size = size / Reduction;&#xD;
		label.getCaption().getFont().setSize(size);&#xD;
	} &#xD;
}

/**
