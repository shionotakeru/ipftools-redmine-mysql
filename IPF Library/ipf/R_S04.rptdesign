  <Script>/**&#xD;
 * Called before rendering each label on a given Axis.&#xD;
 * &#xD;
 * @param axis&#xD;
 *            Axis&#xD;
 * @param label&#xD;
 *            Label&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function beforeDrawAxisLabel( axis, label, icsc )&#xD;
{&#xD;
	importPackage(Packages.org.eclipse.birt.chart.model.component.impl);&#xD;
	importPackage(Packages.org.eclipse.birt.chart.model.data.impl);&#xD;
	importPackage(Packages.org.eclipse.birt.chart.model.attribute);&#xD;
	importPackage(Packages.org.eclipse.birt.chart.model.attribute.impl);&#xD;
	importPackage(Packages.java.text);&#xD;
&#xD;
	// 当日の文字色を赤に変更&#xD;
	if (axis.getOrientation() == Orientation.HORIZONTAL_LITERAL){&#xD;
&#xD;
		var sdf = &quot;&quot;;&#xD;
		var sdf_len = 0;&#xD;
&#xD;
		// パラメータ&quot;SCALE&quot;を取得&#xD;
		var scale = icsc.getExternalContext().getScriptable().getParameterValue(&quot;SCALE&quot;);&#xD;
		// 週の場合は&quot;MM/dd&quot;フォーマット、月の場合は&quot;YYYY/MM&quot;フォーマットに設定&#xD;
		if (scale == 0) {&#xD;
			sdf = new SimpleDateFormat(&quot;MM/dd&quot;);&#xD;
			sdf_len = 5;&#xD;
		} else {&#xD;
			sdf = new SimpleDateFormat(&quot;yyyy/MM&quot;);&#xD;
			sdf_len = 7;&#xD;
		}&#xD;
		&#xD;
		// X軸の日付が当日の場合は赤、それ以外は黒に文字色を設定&#xD;
		if (diffDate(scale, label.getCaption().getValue())) {&#xD;
			label.getCaption().getColor().set(255,0,0);&#xD;
		}else{&#xD;
			label.getCaption().getColor().set(0,0,0);&#xD;
		}&#xD;
		var thedate = sdf.format(new Date(label.getCaption().getValue()));&#xD;
		label.getCaption().setValue(thedate);&#xD;
	}&#xD;
}&#xD;
/**
 * Called before drawing each marker line in an Axis.
 * 
 * @param axis
 *            Axis
 * @param markerLine
 *            MarkerLine
 * @param icsc
 *            IChartScriptContext
 */

function beforeDrawMarkerLine( axis, markerLine, icsc )
{
	importPackage( Packages.org.eclipse.birt.chart.model.attribute );&#xD;
	importPackage( Packages.org.eclipse.birt.chart.model.component.impl );&#xD;
	importPackage( Packages.org.eclipse.birt.chart.model.data.impl );&#xD;
	importPackage( Packages.org.eclipse.birt.chart.util );&#xD;
	importPackage( Packages.java.io );&#xD;
	importPackage( Packages.java.util );&#xD;
&#xD;
	//マーカー情報取得&#xD;
	var max = icsc.getExternalContext().getScriptable().getPageVariable('maxEac') + &quot;&quot;;&#xD;
	var nummax = max - 0;&#xD;
	if (nummax != 0) {&#xD;
		//マーカーを特定するためのラベル取得&#xD;
		var label = markerLine.getLabel().getCaption().getValue();&#xD;
		if(label == &quot;maxeac&quot;){	&#xD;
			var LabelAnchor = markerLine.getLabelAnchor();&#xD;
&#xD;
			var formatter = new java.util.Formatter();&#xD;
			//マーカー位置設定&#xD;
			markerLine.setValue(NumberDataElementImpl.create(max));&#xD;
			markerLine.getLabel().getCaption().setValue(formatter.format(&quot;%1.2f&quot;, nummax));&#xD;
&#xD;
			// データが無い場合はマーカーを消去&#xD;
			if (nummax == 0) {&#xD;
				markerLine.getLineAttributes().setVisible(false);&#xD;
				markerLine.getLabel().setVisible(false);&#xD;
			} else {&#xD;
				markerLine.getLineAttributes().setVisible(true);&#xD;
				markerLine.getLabel().setVisible(true);&#xD;
			}&#xD;
		}&#xD;
	} else {&#xD;
		markerLine.getLineAttributes().setVisible(false);&#xD;
		markerLine.getLabel().setVisible(false);&#xD;
	}&#xD;
}

/**
 * Called before generation of chart model to GeneratedChartState.
 * 
 * @param chart
 *            Chart
 * @param icsc
 *            IChartScriptContext
 */

function beforeGeneration( chart, icsc )
{
	importPackage( Packages.org.eclipse.birt.chart.model.attribute );&#xD;
	importPackage( Packages.org.eclipse.birt.chart.model.component.impl );&#xD;
	importPackage( Packages.org.eclipse.birt.chart.model.data.impl );&#xD;
	importPackage( Packages.org.eclipse.birt.chart.util );&#xD;
	importPackage( Packages.java.io );&#xD;
&#xD;
	var val    = icsc.getExternalContext().getScriptable().getPageVariable('maxEac') - 0;&#xD;
	var maxPV  = icsc.getExternalContext().getScriptable().getPageVariable('maxPV') - 0;&#xD;
	// 予測工数が計画工数を下回った時は計画工数をＹ軸の最大値に設定する&#xD;
	if (val &lt; maxPV) {&#xD;
		val = maxPV;&#xD;
	}&#xD;
	if (val != 0) {&#xD;
		val = (val * 1.01) + &quot;&quot;;	&#xD;
		// x軸取得&#xD;
		var xAxis = chart.getPrimaryBaseAxes()[0];&#xD;
		// 1軸目のy軸取得&#xD;
		var yAxis = xAxis.getAssociatedAxes().get(0);&#xD;
		// スケールの最大値を設定&#xD;
		yAxis.getScale().setMax(NumberDataElementImpl.create(val));&#xD;
	}&#xD;
}
&#xD;
/**&#xD;
 * Called before rendering the Title of an Axis.&#xD;
 * &#xD;
 * @param axis&#xD;
 *            Axis&#xD;
 * @param label&#xD;
 *            Title&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function beforeDrawAxisTitle( axis, label, icsc )&#xD;
{&#xD;
&#xD;
	// フラグ取得&#xD;
	var navi_flg = icsc.getExternalContext().getScriptable().getParameterValue(&quot;NAVI_CREATE_FLAG&quot;);&#xD;
	// 縮小率取得&#xD;
	var Reduction = (icsc.getExternalContext().getScriptable().getPageVariable('Reduction') - 0);&#xD;
	// フラグがfalseになっている場合にフォントサイズを縮小する&#xD;
	if (navi_flg == false) {&#xD;
		var size = label.getCaption().getFont().getSize();&#xD;
		size = size / Reduction;&#xD;
		label.getCaption().getFont().setSize(size);&#xD;
	} &#xD;
}&#xD;


/**&#xD;
 * Called before drawing each marker.&#xD;
 * &#xD;
 * @param marker&#xD;
 *            Marker&#xD;
 * @param dph&#xD;
 *            DataPointHints&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function beforeDrawMarker( marker, dph, icsc )&#xD;
{&#xD;
&#xD;
	// グラフ系列マーカーのサイズ変更&#xD;
	// フラグ取得&#xD;
	var navi_flg = icsc.getExternalContext().getScriptable().getParameterValue(&quot;NAVI_CREATE_FLAG&quot;);&#xD;
	// 縮小率取得&#xD;
	var Reduction = (icsc.getExternalContext().getScriptable().getPageVariable('Reduction') - 0);&#xD;
	// フラグがfalseになっている場合にフォントサイズを縮小する&#xD;
	if (navi_flg == false) {&#xD;
		var size = marker.getSize();&#xD;
		size = size / Reduction;&#xD;
		marker.setSize(size);&#xD;
	} &#xD;
	&#xD;
}&#xD;
&#xD;
function beforeDrawSeries( series, isr, icsc )&#xD;
{&#xD;
&#xD;
	// フラグ取得&#xD;
	var navi_flg = icsc.getExternalContext().getScriptable().getParameterValue(&quot;NAVI_CREATE_FLAG&quot;);&#xD;
	&#xD;
	if(navi_flg == false){&#xD;
		// 縮小時はドリル不可&#xD;
		icsc.getChartInstance().getInteractivity().setEnable(false);&#xD;
	}&#xD;
}&#xD;
&#xD;
// 当日を含む週判定&#xD;
// パラメータの日付から１週間に当日を含む場合はtrue&#xD;
// それ以外はfalseを返却&#xD;
function diffDate(scale, capDate) {&#xD;
&#xD;
	if (scale == 0) {&#xD;
		var diff = BirtDateTime.diffDay(capDate, BirtDateTime.today())&#xD;
		if(diff >= 0 &amp;&amp; diff &lt;= 6){&#xD;
			return true;&#xD;
		} else {&#xD;
			return false;&#xD;
		}&#xD;
	} else {&#xD;
		var sdf = new SimpleDateFormat(&quot;yyyy/MM&quot;);&#xD;
		if (capDate.substring(0, 7) == sdf.format(BirtDateTime.today())) {&#xD;
			return true;&#xD;
		} else {&#xD;
			return false;&#xD;
		}&#xD;
	}&#xD;
}&#xD;
