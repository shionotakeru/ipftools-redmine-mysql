            <xml-property name="queryText"><![CDATA[SELECT	ticket_id									AS ticket_id,
		project_id									AS project_id,
		subject										AS subject,
		str_thedate									AS str_thedate,
		thedate										AS thedate,
		CASE WHEN	planned_value = 0	THEN
			null
		ELSE
			planned_value
		END											AS planned_value,
		CASE WHEN	earned_value = 0	THEN
			null
		ELSE
			earned_value
		END											AS earned_value,
		CASE WHEN	actual_cost = 0	THEN
			null
		ELSE
			actual_cost
		END											AS actual_cost,
		CASE WHEN	estimate = 0	THEN
			null
		ELSE
			estimate
		END											AS estimate,
		parent_id									AS parent_id,
		terminal_flg								AS terminal_flg,
		max_eac							            AS max_eac,
		max_planned_value							AS max_planned_value
	FROM
		(SELECT	ticket_id							AS ticket_id,
				project_id							AS project_id,
				subject								AS subject,
				TO_CHAR(thedate, 'YYYY/MM/DD')		AS str_thedate,
				thedate								AS thedate,
				coalesce(planned_value,0)			AS planned_value,
				CASE WHEN ? = 0	THEN
					coalesce(earned_value_WBS,0)
				ELSE
					coalesce(earned_value_SLOC,0)
				END									AS earned_value,
				actual_cost							AS actual_cost,
				CASE WHEN ? = 0 THEN
					CASE WHEN ? = 0	THEN
						estimate_wbs_special
					ELSE
						estimate_sloc_special
					END
				ELSE
					CASE WHEN ? = 0	THEN
						estimate_wbs_continue
					ELSE
						estimate_sloc_continue
					END
				END									AS estimate,
				parent_id							AS parent_id,
				terminal_flg						AS terminal_flg
			FROM
				(SELECT 0	AS	scale,
						ticket_id,
						project_id,
						subject,
						planned_value,
						actual_cost,
						earned_value_wbs,
						earned_value_sloc,
						eac_wbs_continue,
						eac_wbs_special,
						eac_sloc_continue,
						eac_sloc_special,
						estimate_wbs_continue,
						estimate_wbs_special,
						estimate_sloc_continue,
						estimate_sloc_special,
						thedate,
						parent_id,
						terminal_flg
					FROM	r_s04_w_mart
					UNION
				SELECT	1	AS	scale,
						ticket_id,
						project_id,
						subject,
						planned_value,
						actual_cost,
						earned_value_wbs,
						earned_value_sloc,
						eac_wbs_continue,
						eac_wbs_special,
						eac_sloc_continue,
						eac_sloc_special,
						estimate_wbs_continue,
						estimate_wbs_special,
						estimate_sloc_continue,
						estimate_sloc_special,
						thedate,
						parent_id,
						terminal_flg
					FROM	r_s04_m_mart
				)	r_s04_mart
			WHERE	project_id	=	?
				AND ticket_id	=	?
				AND	CASE WHEN ?	=	0	 THEN
				        TICKET_ID		=	?
					ELSE
					    PARENT_ID		=	?
					END
				AND thedate between ?	AND ?
				AND scale		=	?
		)	r_s04_mart_data
		,
		(SELECT MAX(CASE WHEN ? = 0	THEN
						CASE WHEN ? = 0	THEN
							coalesce(eac_wbs_special,0)
						ELSE
							coalesce(eac_sloc_special,0)
						END
					ELSE
						CASE WHEN ? = 0	THEN
							coalesce(eac_wbs_continue,0)
						ELSE
							coalesce(eac_sloc_continue,0)
						END
					END
				)	AS max_eac,
				MAX(planned_value) AS max_planned_value
			FROM
				(SELECT 0	AS	scale,
						ticket_id,
						project_id,
						subject,
						planned_value,
						actual_cost,
						earned_value_wbs,
						earned_value_sloc,
						eac_wbs_continue,
						eac_wbs_special,
