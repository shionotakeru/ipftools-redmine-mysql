 */&#xD;
&#xD;
function beforeDrawSeries( series, isr, icsc )&#xD;
{&#xD;
	// フラグ取得&#xD;
	var navi_flg = icsc.getExternalContext().getScriptable().getParameterValue(&quot;NAVI_CREATE_FLAG&quot;);&#xD;
	&#xD;
	if(navi_flg == false){&#xD;
		// 縮小時はドリル不可&#xD;
		icsc.getChartInstance().getInteractivity().setEnable(false);&#xD;
	}&#xD;
}&#xD;
&#xD;
&#xD;
/**&#xD;
 * original function : adjustment Xaxis label span.&#xD;
 * &#xD;
 * @param chart&#xD;
 *            Chart&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function setLabelSpan(icsc, chart){&#xD;
	importPackage( Packages.org.eclipse.birt.chart.model.component.impl );&#xD;
	importPackage( Packages.org.eclipse.birt.chart.model.data.impl );&#xD;
	importPackage( Packages.org.eclipse.birt.chart.util );&#xD;
	    &#xD;
    // 最大の期間帯レベル名称文字数&#xD;
    var max = Number(icsc.getExternalContext().getScriptable().getPageVariable('char_count'));&#xD;
	&#xD;
	// 設定するlabel span値&#xD;
	var span = 11 * max; &#xD;
&#xD;
	// フラグ取得&#xD;
	var navi_flg = icsc.getExternalContext().getScriptable().getParameterValue(&quot;NAVI_CREATE_FLAG&quot;);&#xD;
	// 縮小率取得&#xD;
	var Reduction = (icsc.getExternalContext().getScriptable().getPageVariable('Reduction') - 0);&#xD;
	// フラグがfalseになっている場合に設定するlabel spanを縮小する&#xD;
	if (navi_flg == false) {&#xD;
		span = span / Reduction;&#xD;
	} &#xD;
&#xD;
	// x軸のlabel spanを設定&#xD;
	if (max > 0) {&#xD;
		&#xD;
		// x軸取得&#xD;
		var xAxis = chart.getPrimaryBaseAxes()[0];&#xD;
		// label span設定&#xD;
		xAxis.setLabelSpan(span);&#xD;
&#xD;
	}&#xD;
}&#xD;
&#xD;
&#xD;
/**&#xD;
 * original function : adjustment Xaxis label format.&#xD;
 * &#xD;
 * @param axis&#xD;
 *            Axis&#xD;
 * @param label&#xD;
 *            Label&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
function setLabelFormat( axis, label, icsc ){&#xD;
	&#xD;
	/* x軸のラベル改行処理 */&#xD;
	&#xD;
	// x軸名称HashMap&#xD;
	var xaxisMap = (icsc.getExternalContext().getScriptable().getPageVariable('Xaxis_Label'));&#xD;
	// 一行の文字数&#xD;
	var charCount = Number(icsc.getExternalContext().getScriptable().getPageVariable('char_count'));&#xD;
&#xD;
	// Mapから取得した名称に置き換える処理&#xD;
	importPackage(Packages.org.eclipse.birt.chart.model.attribute);&#xD;
	if (axis.getOrientation() == Orientation.HORIZONTAL_LITERAL){&#xD;
		var labelValue = Number(label.getCaption().getValue());&#xD;
		var subject = xaxisMap.get(labelValue);&#xD;
		var newStr = &quot;&quot;;&#xD;
		if(subject != null){&#xD;
			newStr = getDisplayStr(subject,charCount);&#xD;
		}&#xD;
		label.getCaption().setValue(newStr);&#xD;
	}&#xD;
}&#xD;
&#xD;
/**&#xD;
 * original function : get string that does the new line processing and the omission processing. &#xD;
 * &#xD;
 * @param str&#xD;
 *            base string&#xD;
 * @param limit&#xD;
 *            Length limit&#xD;
 *&#xD;
 */&#xD;
function getDisplayStr(str, limit){&#xD;
	&#xD;
	importPackage( Packages.java.lang );&#xD;
	var sb = new StringBuilder(str);&#xD;
	var escapeStr = escape(str);&#xD;
	var normalLen = 0;&#xD;
	var charcLen = 0;&#xD;
	var breakLength = 0;&#xD;
	var omitLength = 0;&#xD;
	&#xD;
	for(var cnt = 0; cnt &lt; escapeStr.length; cnt++){&#xD;
		if(escapeStr.charAt(cnt) == &quot;%&quot;){&#xD;
			if(escapeStr.charAt(++cnt) == &quot;u&quot;){&#xD;
				cnt += 3;&#xD;
				normalLen++;&#xD;
			}&#xD;
			cnt++;&#xD;
		}&#xD;
		normalLen++;&#xD;
&#xD;
		// 改行位置判定&#xD;
		if (breakLength == 0) {&#xD;
			if (normalLen > limit * 2) {&#xD;
				breakLength = charcLen;&#xD;
			}&#xD;
		}&#xD;
		// 最大文字数判定&#xD;
		if (normalLen > limit * 4) {&#xD;
			omitLength = charcLen;&#xD;
			break;&#xD;
		}&#xD;
		charcLen++;&#xD;
	}&#xD;
&#xD;
	// 最大文字数を超える分は省略&#xD;
	if (omitLength != 0) {&#xD;
		sb = new StringBuilder(str.substring(0,omitLength) + &quot;..&quot;);&#xD;
	}&#xD;
	// 改行を設定&#xD;
	if (breakLength != 0) {&#xD;
		sb = sb.insert(breakLength,&quot;\n&quot;);&#xD;
	}&#xD;
&#xD;
	return sb.toString();&#xD;
}</Script>
