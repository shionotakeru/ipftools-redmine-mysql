  <Script>/**&#xD;
 * Called before generation of chart model to GeneratedChartState.&#xD;
 * &#xD;
 * @param chart&#xD;
 *            Chart&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function beforeGeneration( chart, icsc )&#xD;
{&#xD;
	// X軸ラベル領域の高さを設定&#xD;
	setLabelSpan(icsc, chart);&#xD;
	&#xD;
	importPackage( Packages.org.eclipse.birt.chart.model.component.impl );&#xD;
	importPackage( Packages.org.eclipse.birt.chart.model.data.impl );&#xD;
	importPackage( Packages.org.eclipse.birt.chart.util );&#xD;
    &#xD;
    // 最大の達成度／超過度（％）（データセット）&#xD;
    var count = Number(icsc.getExternalContext().getScriptable().getPageVariable('max_value'));&#xD;
&#xD;
	// 達成度／超過度（％）が100を超えた場合はスケールの設定値を変更する処理を行う&#xD;
	if (count > 100) {&#xD;
		&#xD;
		// x軸取得&#xD;
		var xAxis = chart.getPrimaryBaseAxes()[0];&#xD;
		// y軸1を取得&#xD;
		var yAxis1 = xAxis.getAssociatedAxes().get(0);&#xD;
		var yScale1 = yAxis1.getScale();&#xD;
	&#xD;
		// y軸1のMAXを取得&#xD;
		var numMax = yScale1.getMax().getValue();&#xD;
		&#xD;
		&#xD;
		//--- スケールに設定するMAX値、STEP値を計算 ---//&#xD;
		// 基本とするstep数&#xD;
		var stepNum = 5;&#xD;
		// 増やす分のstep数&#xD;
		var addStepNum = 1;&#xD;
		// 新しく設定するstep size&#xD;
		var newStepNum = BirtMath.roundDown(count/stepNum);&#xD;
		// 新しく設定するmax値&#xD;
		var newNumMax = BirtMath.roundDown(newStepNum * (stepNum + addStepNum));&#xD;
	&#xD;
		// --y軸1のスケールの設定値を変更&#xD;
		yScale1.setStep(newStepNum);&#xD;
		yScale1.setMax(NumberDataElementImpl.create(newNumMax));&#xD;
		yAxis1.setScale(yScale1);&#xD;
	&#xD;
	}&#xD;
	&#xD;
}&#xD;
&#xD;
&#xD;
/**&#xD;
 * Called before rendering the Title of an Axis.&#xD;
 * &#xD;
 * @param axis&#xD;
 *            Axis&#xD;
 * @param label&#xD;
 *            Title&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function beforeDrawAxisTitle( axis, label, icsc )&#xD;
{&#xD;
	// 軸タイトルのフォントサイズ変更&#xD;
	// フラグ取得&#xD;
	var navi_flg = icsc.getExternalContext().getScriptable().getParameterValue(&quot;NAVI_CREATE_FLAG&quot;);&#xD;
	// 縮小率取得&#xD;
	var Reduction = (icsc.getExternalContext().getScriptable().getPageVariable('Reduction') - 0);&#xD;
	// フラグがfalseになっている場合にフォントサイズを縮小する&#xD;
	if (navi_flg == false) {&#xD;
		var size = label.getCaption().getFont().getSize();&#xD;
		size = size / Reduction;&#xD;
		label.getCaption().getFont().setSize(size);&#xD;
	} &#xD;
}&#xD;
&#xD;
&#xD;
/**&#xD;
 * Called before rendering each label on a given Axis.&#xD;
 * &#xD;
 * @param axis&#xD;
 *            Axis&#xD;
