						ELSE
							MART.PARENT_ID	=	?
					END
			GROUP BY 
				MART.TICKET_ID,
				MART.SUBJECT
		) TABLE1
) > 0 
	THEN UNION_TABLE.NO_DATA_FLG = '0'
