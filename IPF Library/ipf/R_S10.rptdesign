            <xml-property name="queryText"><![CDATA[SELECT
	MART.TICKET_ID AS TICKET_ID
	,MART.SUBJECT AS SUBJECT
	,CASE WHEN MART.CLOSED_FLG = '0'
		THEN
			CASE WHEN MART.TEST_ITEM_COUNT IS NULL OR MART.TEST_ITEM_COUNT = 0
				THEN 
					(CAST(MART.BUG_COUNT AS DOUBLE PRECISION)/ (CAST(MART.ACTUAL_SLOC AS DOUBLE PRECISION) * MART.BUG_DENSITY)) * 100
				ELSE
					((CAST(MART.BUG_COUNT AS DOUBLE PRECISION)/ (CAST(MART.ACTUAL_SLOC AS DOUBLE PRECISION) * MART.BUG_DENSITY)) * (CAST(MART.ACTUAL_TEST_COUNT AS DOUBLE PRECISION) / CAST(MART.TEST_ITEM_COUNT AS DOUBLE PRECISION))) * 100
				END

		ELSE 0
		END AS UN_CLOSED
	,CASE WHEN MART.CLOSED_FLG = '1'
		THEN
			CASE WHEN MART.TEST_ITEM_COUNT IS NULL OR MART.TEST_ITEM_COUNT = 0
				THEN 
					(CAST(MART.BUG_COUNT AS DOUBLE PRECISION)/ (CAST(MART.ACTUAL_SLOC AS DOUBLE PRECISION) * MART.BUG_DENSITY)) * 100
				ELSE
					((CAST(MART.BUG_COUNT AS DOUBLE PRECISION)/ (CAST(MART.ACTUAL_SLOC AS DOUBLE PRECISION) * MART.BUG_DENSITY)) * (CAST(MART.ACTUAL_TEST_COUNT AS DOUBLE PRECISION) / CAST(MART.TEST_ITEM_COUNT AS DOUBLE PRECISION))) * 100
				END

		ELSE 0
		END AS CLOSED
	,MART.BUG_DENSITY AS BUG_DENSITY
	,MART.TERMINAL_FLG
	,MART.PARENT_ID
FROM
	R_S10_MART MART
WHERE
	MART.PROJECT_ID	= ?
	AND CASE WHEN ? = 0
			THEN 
				MART.TICKET_ID =  ?
			ELSE
				MART.PARENT_ID	= ? 
		END
ORDER BY MART.TICKET_ID		
OFFSET ? LIMIT ?]]></xml-property>
